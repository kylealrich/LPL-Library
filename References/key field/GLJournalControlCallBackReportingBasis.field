			invoke CreateJournalControlRefPerLedgerSystem ReportingBasis 
				invoked.PrmAccountingEntity 	= TransientAccountingEntity
				invoked.PrmStartDateJulian  	= TransientStartDateJulian
				invoked.PrmEndDateJulian 		= TransientEndDateJulian
				invoked.PrmBasisYear			= TransientBasisYear
				invoked.PrmFirstProcessing		= false
				invoked.PrmLedger				= TransientLedger
				invoked.PrmStartingNumber		= TransientStartingNumber
			return true
