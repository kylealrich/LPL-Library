    Field Rules
    
 		Ledger
			if (ToAccountingEntity entered
			or (AccountingUnit entered
			and FinanceEnterpriseGroup.AccountingUnitRequired)
			or (FinanceDimension1 entered
			and FinanceEnterpriseGroup.FinanceDimension1Required)
			or  GeneralLedgerChartAccount entered)
				default to FinanceEnterpriseGroup.CoreLedger
			else
				initialize Ledger				

			constraint (Ledger = FinanceEnterpriseGroup.CoreLedger)
				"Ledger<Ledger>MustBeCore"	
				
		ToAccountingEntity
			if (Ledger entered)
				required
					"<FinanceEnterpriseGroup.AccountingEntityLabel>IsRequired"

			if (ToAccountingEntity entered)
				LocalCodeBlockRelation	= CodeBlockRelationForLedgerEntityRel.CodeBlockRelation
				LocalKeyFromValue		= Ledger
				LocalKeyToValue			= ToAccountingEntity
				constraint (CodeBlockRelationDetailRel exists)
					"Invalid<FinanceEnterpriseGroup.LedgerMessage>_<Ledger>ForAccountingEntity<ToAccountingEntity>"

				if (!BypassStructureRelationEdit)
					LocalEditMode			= true		
					LocalNumericValue2 = DefaultCodeBlockValues  
					LocalEditMode			= false
	
				if (!BypassStructureRelationEdit)
					if (Ledger entered)
						constraint (Ledger exists)
							"Ledger<Ledger>DoesNotExist"
					if (ToAccountingEntity entered)
						constraint (ToAccountingEntity exists)
							"<FinanceEnterpriseGroup.AccountingEntityLabel>_<ToAccountingEntity>DoesNotExist"
					if (AccountingUnit entered)
						constraint (AccountingUnit exists)
							"<FinanceEnterpriseGroup.AccountingUnitLabel>_<AccountingUnit>DoesNotExist"
					if (GeneralLedgerChartAccount entered)
						constraint (GeneralLedgerChartAccount exists)
							"<FinanceEnterpriseGroup.AccountLabel>_<GeneralLedgerChartAccount>DoesNotExist"
					if (Project entered)
						constraint (Project exists)
							"<FinanceEnterpriseGroup.ProjectLabel>_<Project>DoesNotExist"
					if (FinanceDimension1 entered)
						constraint (FinanceDimension1 exists)
							"<FinanceEnterpriseGroup.FinanceDimension1Label>_<FinanceDimension1>DoesNotExist"
					if (FinanceDimension2 entered)
						constraint (FinanceDimension2 exists)
							"<FinanceEnterpriseGroup.FinanceDimension2Label>_<FinanceDimension2>DoesNotExist"
					if (FinanceDimension3 entered)
