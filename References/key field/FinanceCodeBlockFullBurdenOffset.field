							"<FinanceEnterpriseGroup.FinanceDimension3Label>_<FinanceDimension3>DoesNotExist"
					if (FinanceDimension4 entered)
						constraint (FinanceDimension4 exists)
							"<FinanceEnterpriseGroup.FinanceDimension4Label>_<FinanceDimension4>DoesNotExist"
					if (FinanceDimension5 entered)
						constraint (FinanceDimension5 exists)
							"<FinanceEnterpriseGroup.FinanceDimension5Label>_<FinanceDimension5>DoesNotExist"
					if (FinanceDimension6 entered)
						constraint (FinanceDimension6 exists)
							"<FinanceEnterpriseGroup.FinanceDimension6Label>_<FinanceDimension6>DoesNotExist"
					if (FinanceDimension7 entered)
						constraint (FinanceDimension7 exists)
							"<FinanceEnterpriseGroup.FinanceDimension7Label>_<FinanceDimension7>DoesNotExist"
					if (FinanceDimension8 entered)
						constraint (FinanceDimension8 exists)
							"<FinanceEnterpriseGroup.FinanceDimension8Label>_<FinanceDimension8>DoesNotExist"
					if (FinanceDimension9 entered)
						constraint (FinanceDimension9 exists)
							"<FinanceEnterpriseGroup.FinanceDimension9Label>_<FinanceDimension9>DoesNotExist"
					if (FinanceDimension10 entered)
						constraint (FinanceDimension10 exists)
							"<FinanceEnterpriseGroup.FinanceDimension10Label>_<FinanceDimension10>DoesNotExist"
					LocalEditMode			= true
					LocalNumericValue		= EditCodeBlockRelationDetail

		AccountingUnit
			if (Ledger entered)
				if (FinanceEnterpriseGroup.AccountingUnitRequired)
					required
						"<FinanceEnterpriseGroup.AccountingUnitLabel>IsRequired"
			else
				cannot be entered
					"EnterAllRequiredDimensionsOrLeaveAllDimensionsBlank"
			constraint (AccountingUnit.AccountingUnitType.Posting)
				"<FinanceEnterpriseGroup.AccountingUnitLabel>_<AccountingUnit>MustBePostingType"

		GeneralLedgerChartAccount
			if (Ledger entered)
				required
					"<FinanceEnterpriseGroup.AccountLabel>IsRequired"
			else
				cannot be entered
					"EnterAllRequiredDimensionsOrLeaveAllDimensionsBlank"
			constraint (GeneralLedgerChartAccount.ChartType.Posting)
				"<FinanceEnterpriseGroup.AccountLabel>_<GeneralLedgerChartAccount>MustBePostingType"

		Project
			if (Ledger entered)
				if (FinanceEnterpriseGroup.ProjectRequired)
					required
						"<FinanceEnterpriseGroup.ProjectLabel>IsRequired"
				else
				if (GeneralLedgerChartAccount.RequireProject)
					required
						"<FinanceEnterpriseGroup.AccountLabel>_<GeneralLedgerChartAccount>_-_<FinanceEnterpriseGroup.ProjectLabel>_isRequired"
				else
				if (FinanceEnterpriseGroup.ValidationDimensionRequiresProject)
					if (FinanceEnterpriseGroup.StructureValidationDimension.Dimension3
					and FinanceDimension3 entered)
						required
							"<FinanceEnterpriseGroup.FinanceDimension3Label>_<FinanceDimension3>_-_<FinanceEnterpriseGroup.ProjectLabel>_isRequired"
					if (FinanceEnterpriseGroup.StructureValidationDimension.Dimension4
					and FinanceDimension4 entered)
						required
							"<FinanceEnterpriseGroup.FinanceDimension4Label>_<FinanceDimension4>_-_<FinanceEnterpriseGroup.ProjectLabel>_isRequired"
			else
				cannot be entered
					"EnterAllRequiredDimensionsOrLeaveAllDimensionsBlank"				
			constraint (Project.ProjectType.Posting)
				"<FinanceEnterpriseGroup.ProjectLabel>_<Project>MustBePostingType"
			if (!BypassStructureRelationEdit
			and FinanceEnterpriseGroup.ProjectStructureValidation
			and Project entered)
