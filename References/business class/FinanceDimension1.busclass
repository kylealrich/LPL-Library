						invoke SetLevel
					LocalFinanceDimension1 = FinanceDimension1
					include CreateShadowForDimension
					if (DimensionType.Posting)
						initialize ParentFinanceDimension1


						
				if (DimensionType.Posting)
					if (!BypassNewStructureNotification)
						invoke NotifyNewStructureComponent FinanceEnterpriseGroup
							invoked.PrmNewDimension1			= FinanceDimension1
                
                include CheckSystemDimensions															
		CreateNoRules is a Create Action
			restricted
			bypass field rules
		
			Entrance Rules
				include CheckValidContext			
			Exit Rules

				
		Update is an Update Action
		    
			Local Fields
				LocalIsAZoneDimension		is Boolean
			Action Rules
			    TransientIsAZoneDimension = IsAZoneDimension
				if (IsAZoneDimension)
					invoke CheckSiblingForZone FinanceDimension1
					
				LocalIsAZoneDimension		= DerivedIsAZoneDimension
				UseInputValue = true
				
				if (LocalIsAZoneDimension
				and !IsAZoneDimension)
					ParentDimension		= DerivedParentDimension
					ZoneDimension		= ParentDimension.ZoneDimension
				if (DimensionType.Posting
				and IsAZoneDimension)
					ZoneDimension = FinanceDimension1 
					
			Exit Rules
				if (DefaultZoneDimension)
					if (ZoneDimension 	= blank
					and DimensionType.Posting)
						ZoneDimension	= FinanceDimension1
												
				if (!BypassUpdateStructure)
