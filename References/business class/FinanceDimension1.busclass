				constraint (ZoneDimension.DimensionType.Posting)
					"ZoneDimensionMustBePostingType"				
				if (ZoneDimension entered 
				and IsAZoneDimension) 
					LocalFinanceDimension1 = ZoneDimension			
					TopNodeFound   		   = false
					ZoneWithinNode		   = false
					while (!TopNodeFound)
						if (LocalFinanceDimension1 = FinanceDimension1)
							TopNodeFound   = true
							ZoneWithinNode = true
						else
						if (LocalFinanceDimension1.DimensionType.Node
						or  LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or !LocalFinanceDimension1.DimensionInContextStructureRel exists)					
							TopNodeFound = true
						else
							LocalFinanceDimension1 = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension					
					constraint (ZoneWithinNode)
						"OnlyDescendantsCanBeZone<FinanceEnterpriseGroup.FinanceDimension1Label>"
    	DefaultZoneDimension
