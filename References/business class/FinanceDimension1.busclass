		Move is an Instance Action
			valid when (MovableSummaryOrPostingDimension)
			
			Parameters
				PrmContextDimension is a FinanceDimension1ContextValue
				NewParent 			is a FinanceDimension1
							
			Parameter Rules
				PrmContextDimension
					initial value is FinanceDimension1
				NewParent
					default to DerivedTopNode
					required
					constraint (NewParent != FinanceDimension1)
						"CannotMoveToSelf"
					constraint (NewParent.DimensionType.Summary
					or			NewParent.DimensionType.Node)
						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension1Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<NewParent.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension1Label>"	

					TopNodeFound = false
					LocalFinanceDimension1 = NewParent
					if (!NewParent.DimensionType.Node)
						while (!TopNodeFound)
							constraint (FinanceDimension1 != LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension)
								"<FinanceDimension1>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension1 not entered)
								TopNodeFound = true
							else
								LocalFinanceDimension1 = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension	
					constraint (NewParent != NewParent.ZoneDimension)
						"<NewParent>CannotBeASummaryLevelZone<FinanceEnterpriseGroup.FinanceDimension1Label>"						

					constraint (FinanceDimension1 != ZoneDimension)
						"ZoneCannotBeMoved-<FinanceEnterpriseGroup.FinanceDimension1Label>"	

													
			Local Fields
				OldParent is like FinanceDimension1
																		
			Entrance Rules
				include CheckValidContext
				
				OldParent				 = DerivedParentDimension
				ParentDimension			 = NewParent

			Action Rules
							
				ParentDimension  	 = NewParent

				invoke Update DimensionInContextStructureRel 
					invoked.ParentDimension		 	= NewParent
					invoked.ZoneDimension			= NewParent.ZoneDimension					

			Exit Rules
 				LocalFinanceDimension1 = FinanceDimension1
 							
				include CreateShadowForDimension		
				if (DimensionType.Summary)
					TopNodeFound = false
					while (!TopNodeFound)
						if (LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DimensionType.Summary)
							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
							if (LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier entered)
								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension1 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension1 = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
				invoke NotifyMoveDimension1 NotifyBudgetTemplateRel
					invoked.PrmFinanceDimension1 = FinanceDimension1
					invoked.PrmNewParent		 = NewParent
					invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
