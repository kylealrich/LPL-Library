						else
							constraint (BudgetIdentifier != LocalFinanceDimension1.BudgetIdentifier) 
								"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension1.DisplayDimension>"
							if (LocalFinanceDimension1.DimensionType.Node
