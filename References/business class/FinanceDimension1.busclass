		RequestNewFinanceDimension1 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup		
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
				DimensionType
			    FinanceDimension1Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension1Label>Structure"
				DisplayDimension 				is like FinanceDimension1
					translatable
				PrmFinanceDimension1			is like FinanceDimension1
				ParentFinanceDimension1			is a FinanceDimension1
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension1Structure
					initial value is first FinanceEnterpriseGroup.EnterpriseDim1Structure
					default to first FinanceEnterpriseGroup.EnterpriseDim1Structure
				DisplayDimension
					default to PrmFinanceDimension1		
				PrmFinanceDimension1
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension1Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension1RegularExpression entered
					and DimensionType.Posting)
						constraint (PrmFinanceDimension1 matches FinanceEnterpriseGroup.FinanceDimension1RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension1Label>"
					LocalStructure					= FinanceDimension1Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= DisplayDimension
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension1>"
				DimensionType
	    			default to CreateDimensionType		
					required
				BudgetIdentifier
					constraint (DimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"		
				IncludeInBudgetEdit
					constraint (DimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalStructure					= FinanceDimension1Structure
					constraint (!RequestSystemDimension1Rel exists)
						"<first RequestSystemDimension1Rel.DisplayDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension1Label>"
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension1Label>"		
				Description
					required
				Active
					initial value is true
		
			Action Rules
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension1			= PrmFinanceDimension1
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension1
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension1 entered)
						invoked.ParentDimension			= ParentFinanceDimension1
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
					invoked.ZoneDimension 				= ParentFinanceDimension1.ZoneDimension
