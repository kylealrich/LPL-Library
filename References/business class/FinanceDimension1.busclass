			if (FinanceEnterpriseGroup.BODTrigger)
				Service		= "FinanceDimension1Service"
				if (action type.Create)
					ActionCode	= ActionCode.Create
				else
					ActionCode	= ActionCode.Update				
				increment bod id.VariationID






				LocalBODCurrentTimeStamp.CurrentTimeStamp = current timestamp
				include TriggerFinanceDimension1BOD



				
	
		CheckValidContext	
			if (FinanceDimension1Structure.UnusedDimensionStructure)
				constraint (InvokedByUnusedStructure
				or 			LocalInvokedByUnusedStructure)
					"ActionNotValidForStructure<FinanceDimension1Structure>"
						
		CreateShadowForDimension
			LocalChildDimension = LocalFinanceDimension1
			TopNodeFound  	 	= false
			invoke Delete DimensionShadowRel
			while (!TopNodeFound)
				invoke Create FinanceDimension1Shadow
					invoked.FinanceEnterpriseGroup 	   = FinanceEnterpriseGroup
					invoked.FinanceDimension1Structure = FinanceDimension1Structure
					invoked.FinanceDimension1		   = LocalChildDimension
					invoked.DimensionType			   = LocalChildDimension.DimensionType
					invoked.SummaryFinanceDimension1   = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension

				if (LocalFinanceDimension1.DimensionType.Summary)
					invoke SetLevel LocalFinanceDimension1
				if (LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
				or  LocalFinanceDimension1 not entered)
					TopNodeFound = true
				else
					LocalFinanceDimension1 = LocalFinanceDimension1.DimensionInContextStructureRel.ParentDimension			

		CheckSystemDimensions
			if (FinanceDimension1Structure = FinanceEnterpriseGroup.EnterpriseDim1Structure)
				if (EnterpriseSystemDim1Rel exists)
					SystemDimDefined	     = true
				if (EnterpriseDefaultZoneDim1Rel exists)
					DefaultZoneDimDefined    = true
				if (EnterpriseZoneDim1Rel exists)
					EnterpriseZoneDimDefined = true
				
				invoke SetSystemDimensionFlags FinanceEnterpriseGroup
					invoked.PrmDimension			    = 1
					invoked.PrmSystemDimDefined	        = SystemDimDefined
					invoked.PrmDefaultZoneDimDefined    = DefaultZoneDimDefined
					invoked.PrmEnterpriseZoneDimDefined = EnterpriseZoneDimDefined
				
		DeleteRules
			if (FinanceEnterpriseGroup.BODTrigger)
				Service			= "FinanceDimension1Service"
				ActionCode		= ActionCode.Delete
				increment bod id.VariationID






				LocalBODCurrentTimeStamp.CurrentTimeStamp = current timestamp
				include TriggerFinanceDimension1BOD



				

    Apply Pending Effective Rules
		if (FinanceEnterpriseGroup.BODTrigger and !LocalBODTrigger)
			Service			= "FinanceDimension1Service"
			if (action != "UpdateBODIdFields") 
				ActionCode = ActionCode.Update
				if (action type.Create)
					ActionCode = ActionCode.Create
				else
					if (action type.Delete)
						ActionCode = ActionCode.Delete
				LocalBODCurrentTimeStamp.CurrentTimeStamp = current timestamp
				increment bod id.VariationID






				include TriggerFinanceDimension1BOD



	Audit Entry Rules
		if(not audit period.Future)
			if (FinanceEnterpriseGroup.BODTrigger and !LocalBODTrigger)
				Service			= "FinanceDimension1Service"
				if (!action type.Delete)
					LocalBODCurrentTimeStamp.CurrentTimeStamp = current timestamp
					include TriggerFinanceDimension1BOD



    
