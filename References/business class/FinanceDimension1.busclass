FinanceDimension1 is a BusinessClass
	owned by GeneralLedger
	prefix is FDIM1
	representative text is "<DerivedRepresentativeText>"   

    
    Ontology
    	symbolic key is FinanceDimension1

    Patterns
        implements BODId    
		implements ChildHierarchy
			children are DimensionChildrenRel
			child set is DimensionHierarchyRel
				child is FinanceDimension1
			top node when (DimensionType.Node)
			descendants are DescendantsRel
		implements InlineUserFields
			size is 3000			

        
   	Context Fields
		ContextDimension			is a FinanceDimension1ContextValue
		FinanceDimension1Structure
		InvokedByUnusedStructure
		AllocationContext
		BypassActiveCodeBlockEdit
#ifdef module integration
		FSMInboundBODTracker
#endif
		
    Persistent Fields
		Description
		DimensionType
		DisplayDimension			is like FinanceDimension1
			translatable			
		Active
		SequenceNumber				is Numeric 7
    	OldSystemDimension			is Boolean
    	OldIsAZoneDimension			is Boolean
    	OldZoneDimension			is like FinanceDimension1
    	OldEnterpriseZoneDimension	is Boolean
    	OldDefaultZoneDimension		is Boolean
    	BudgetIdentifier
    	IsTopNode					is Boolean    	
    	Level						is Numeric 3						
		ParentFinanceDimension1		is like FinanceDimension1           
		DimensionStructure			is like FinanceDimension1Structure  
		AddressCode
 		FundType				
			
			
	Transient Fields
		TransientContextDimension is a FinanceDimension1ContextValue
			derive value from FinanceDimension1
		ParentDimension				is a FinanceDimension1
			derive value from DimensionInContextStructureRel.ParentDimension
    	SystemDimension				is Boolean
    		derive value from DerivedSystemDimension
    	IsAZoneDimension			is Boolean
    		derive value from DerivedIsAZoneDimension
    	ZoneDimension				is a FinanceDimension1
    		derive value from DerivedZoneDimension
    	EnterpriseZoneDimension	is Boolean
    		derive value from DerivedEnterpriseZoneDimension
    	DefaultZoneDimension		is Boolean
    		derive value from DerivedDefaultZoneDimension
    	IncludeInBudgetEdit		is Boolean
    		derive value from DerivedIncludeInBudgetEdit  
 		SkipHierarchy				is Boolean
		InvokedByCreateWithin		is Boolean
		SkipPropagate				is Boolean
		NewDimension		 		is like FinanceDimension1
		BypassNewStructureNotification is Boolean
		BypassUpdateStructure		is Boolean
		CreateDimensionType			is Numeric 2
			States
	    		Summary	value is 2
	    		Posting	value is 1
	    	derive value from DimensionType
	    TransientIncludeOrExclude       is Boolean	
	    TransientIsAZoneDimension		is Boolean	
	    TransientIncludeInBudgetEdit    is Boolean
	    									   				
	Conditions
		PostingDimension
			restricted
			when (DimensionType.Posting)
		SummaryDimension
			restricted
			when (DimensionType.Summary)
		SummaryOrPostingDimension
			restricted
			when (!DimensionType.Node)
		SummaryOrPostingInStructure
			restricted
			when (SummaryOrPostingDimension
			and   DimensionInContextStructureRel exists)
		MovableSummaryOrPostingDimension
			restricted
			when (!DimensionType.Node
			and   !ZoneDimension = FinanceDimension1
			and   !EnterpriseZoneDimension)			
		CanDefineZones
			restricted
			when (FinanceEnterpriseGroup.Dimension1ZoneBalancing)
		DefinedAsAZone
			restricted
			when (IsAZoneDimension)
		IsAchild
			restricted
			when (DimensionInContextStructureRel exists)
		DimensionDescendants 
			restricted
			when (DimensionType.Posting
			and   !IsAZoneDimension			
			and  (FinanceDimension1 	= ContextDimension
			or    DescendantOfContextDimensionRel exists))
		DimensionIsZone
			restricted
			when (FinanceDimension1 = ZoneDimension
			or	  EnterpriseZoneDimension)
		EligibleParent
			restricted
			when (DimensionType.Summary
			and   DimensionStructure = FinanceDimension1Structure
			and   FinanceDimension1 != ContextDimension
			and   FinanceDimension1 != ContextDimensionRel.ParentDimension
			and   !DescendantOfContextDimension)
		ZoneDimension1
			restricted
			when (IsADerivedZoneFinanceDimension1)
		RecordExists
			restricted
			when (FinanceDimension1 exists)
		HasChildren
			restricted
			when (DimensionHierarchyRel exists)
		IsASystemDimension
			restricted
			when (SystemDimension)
		NodeDimension
			restricted
			when (DimensionType.Node)
		SummaryDimensionOrNodeInFinanceDimension1Structure
			restricted
			when ((FinanceDimension1 != ContextDimension)
			and   (DimensionStructure = FinanceDimension1Structure
			and   (DimensionType.Node)
			or    (DimensionType.Summary
			and    HelperListDimensionRel exists)))
		SummaryInStructure
			restricted
			when (FinanceDimension1 != ContextDimension
			and   DimensionType.Summary			
			and   DimensionInContextStructureRel exists
			and   HelperListDimensionRel exists)
		HelperListDimension
			restricted
			when (DimensionType.Posting
			and  !HelperListDimensionRel exists)
		NotUsedInHierarchy
			restricted
			when (DimensionType.Posting
			and   !DimensionInContextStructureRel exists)
		NotUsedInAStructure
			restricted
			when (DimensionType.Posting
			and   !FinanceDimension1Hierarchy(FinanceDimension1) set exists)
		SummaryWithinTopNode
			restricted
			when (!DimensionType.Posting
			and   DimensionStructure = FinanceDimension1Structure)	
		SummaryInEnterpriseStructure
			restricted
			when (DimensionType.Summary
			and   DimensionStructure = FinanceEnterpriseGroup.EnterpriseDim1Structure)
		FutureChanges
			restricted
			when (this instance.has future changes)
		EligibleToRemove
			restricted
			when (!DimensionChildrenRel exists		

			and   !ZoneDimension = FinanceDimension1)
		EligibleToDelete
			restricted
			when (DimensionType.Posting
			and  !UsedByATransaction)
				
		SystemDimension1
			when (DimensionType.Posting
			and   SystemDimension)			
		
#ifdef module ad
		ADBPostingDimension1
			restricted
			when (ADBAccountMasterRel exists
			and   DimensionType.Posting)
#endif

		RemovePostingDimension1
			restricted
			when (DimensionType.Posting
			and  !ZoneDimension = FinanceDimension1)

		ActionCreate
			when (action = "create")
		ActionUpdate
			when (!action = "create"
			and	!action = "Delete")
		ActionDelete
			when (action = "Delete")
		FinanceEnterpriseGroupUpdate
			when (!FinanceEnterpriseGroup = old FinanceEnterpriseGroup)
		DimensionTypeUpdate
			when (!DimensionType = old DimensionType)
		DisplayDimensionUpdate
			when (!DisplayDimension = old DisplayDimension)
		DescriptionUpdate
			when (!Description = old Description)
		ActiveUpdate
			when (!Active = old Active)
		BudgetIdentifierUpdate
			when (!BudgetIdentifier = old BudgetIdentifier)
		AddressCodeUpdate
			when (!AddressCode = old AddressCode)
		SequenceNumberUpdate
			when (!SequenceNumber = old SequenceNumber)
		LevelUpdate
			when (!Level = old Level)
		IsTopNodeUpdate
			when (!IsTopNode = old IsTopNode)
		ParentFinanceDimension1Update
			when (!ParentFinanceDimension1 = old ParentFinanceDimension1)
		DimensionStructureUpdate
			when (!DimensionStructure = old DimensionStructure)					
		SecurityGroupAllowsAccess
			when ((FinanceEnterpriseGroup.FinanceDimension1Entry = false)
			or    (actor.context.FinanceDimension1SecurityGroup = "")      
			or    (actor.context.FinanceDimension1SecurityGroup != ""
			and   (DimensionType.Node
			or     BypassActiveCodeBlockEdit
			or    (SecurityGroupDetailRel exists
			and   !SecurityGroupDetailRel.CreatedForHierarchyAccess))))

		SecurityGroupAllowsMaintenance
			when (LocalSecurityGroupAllowsMaintenance)
		UsedForFundAccounting 
			when (FinanceEnterpriseGroup.FundAccounting = true
			and   FinanceEnterpriseGroup.FundDimension.Dimension1)
				
	Local Fields
		LocalSecurityGroupAllowsMaintenance is Boolean
		UseInputValue					is Boolean
		RecordAdded						is Boolean
		BudgetIdentifierArray
		I1								is Numeric 2
		I2								is Numeric 2
		BusinessClass					is Alpha size 60
		System							is a GeneralLedgerSystemCode
		ActionCode
		Service							is Alpha size 30
		LocalFinanceDimension1			is a FinanceDimension1
		LocalParentFinanceDimension1	is a FinanceDimension1
		LocalReportDimension			is a FinanceDimension1
		LocalUpdateDimension			is a FinanceDimension1
		LocalChildDimension				is a FinanceDimension1
		TopNodeFound					is Boolean
		ZoneWithinNode					is Boolean
		LevelCalcDimension				is a FinanceDimension1
		LocalLevel						is Numeric 3
		LocalStructure					is like FinanceDimension1Structure
		LocalUpdateFinanceDimension1	is a FinanceDimension1
		SystemDimDefined				is Boolean
		DefaultZoneDimDefined 			is Boolean
		EnterpriseZoneDimDefined 		is Boolean
		LocalBODCurrentTimeStamp		is a BODCurrentTimeStamp
		LocalInvokedByUnusedStructure 	is Boolean
		LocalNativeLPLBODTrigger		is Boolean				
		LocalBodAccountingEntity		is AlphaUpper size 12
		LocalCrossAccountingEntity 		is Alpha 40
		LocalCleanDocIDAccountingentity 	is Alpha 40
		LocalBODFormattedID				is Alpha 100
		FinanceDimensionCleanDocIDXMLBOD	is XMLDocument
		LocalCodeBlockRelation			is a CodeBlockRelation
		LocalKeyFromValue				is Alpha 200
		LocalKeyToValue					is Alpha 200
		TemplatesWithIdentifierCount	is Numeric size 6
		LocalFinanceEnterpriseGroup		is like FinanceEnterpriseGroup
		LocalDisplayDimension			is like FinanceDimension1		
#ifdef module integration
		NewBODTracker  				is a FSMInboundBODTracker view
#endif
		LocalFSMInboundBODTracker	is Numeric 15
		Error            			is Boolean
	    ErrorMessage     			is Alpha 300
	    LocalBODTrigger				is Boolean
		LocalConfigurationParameter	is Alpha size up to 200
		LocalSourceValue			is Alpha 30
		
	Derived Fields
		DerivedSecurityGroupAllowsAccess is a DerivedField
			type is Boolean
			if (DimensionType.Node)
				LocalSecurityGroupAllowsMaintenance = true
				return true
			if (ActorSecurityGroup not entered)
				LocalSecurityGroupAllowsMaintenance = true
				return true
			if (SecurityGroupDetailRel exists)
				if (parentcontext.name != "AccountAnalysisSettings")
					if (!SecurityGroupDetailRel.CreatedForHierarchyAccess)
						LocalSecurityGroupAllowsMaintenance = true
					return true
				else
					if (!SecurityGroupDetailRel.CreatedForHierarchyAccess)
						return true	
			return false
						
 		DerivedRepresentativeText	is a DerivedField
			type is Alpha 120
			restricted
			if (IsTopNode)
				return Description
			else				
				return DerivedRepresentativeTextString
		DerivedRepresentativeTextString is a StringField	
			type is Alpha 120
			restricted
			DisplayDimension
			" - "
			Description
		DerivedShadowCount 				is a DerivedField
			type is Numeric 8
			restricted
			return (instance count of FinanceDimension1Shadow(SummaryFinanceDimension1) set)
		
    	DerivedSystemDimension			is a DerivedField
    		type is Boolean
    		restricted
    		if (UseInputValue)
    			return false
   			else
   				return DimensionInContextStructureRel.SystemDimension
    	DerivedIsAZoneDimension			is a DerivedField
    		type is Boolean
    		restricted
    		if (UseInputValue)
    			return false
    		else
   				return DimensionInContextStructureRel.IsAZoneDimension
    	DerivedZoneDimension			is a DerivedField
    		type is like FinanceDimension1
    		restricted
    		if (UseInputValue)
    			return blank
    		else
    			return DimensionInContextStructureRel.ZoneDimension
    	IsADerivedZoneFinanceDimension1		is a DerivedField
    		type is Boolean
    		restricted
    		if (UseInputValue)
    			return false
    		else
    			return DimensionInEnterpriseStructureRel.IsAPostingZoneDimension
    	DerivedEnterpriseZoneDimension	is a DerivedField
    		type is Boolean
    		restricted
    		if (UseInputValue)
    			return false
    		else
    			return DimensionInContextStructureRel.EnterpriseZoneDimension
    	DerivedDefaultZoneDimension		is a DerivedField
    		type is Boolean
    		restricted
    		if (UseInputValue)
    			return false
    		else
    			return DimensionInContextStructureRel.DefaultZoneDimension
		DerivedParentDimension  is a DerivedField
			type is like FinanceDimension1
			restricted
			if (action type.Create)
				return ParentFinanceDimension1	
			else		
				return DimensionInContextStructureRel.ParentDimension
		DerivedIncludeInBudgetEdit is a DerivedField
			type is Boolean
			restricted
    		if (UseInputValue)
    			return false
    		else
    			return DimensionInContextStructureRel.IncludeInBudgetEdit						
		DerivedSequenceNumber	is a DerivedField
			type is AlphaRight 7
			restricted
			return DerivedAlphaSequence[2:8]
		DerivedAlphaSequence	is a DerivedField
			type is AlphaRight 8
			restricted
			return (10000000 + SequenceNumber)
		StructureDimensionExists 	 is a DerivedField
			type is Boolean
			restricted
			if (DimensionInContextStructureRel exists)
				return true
		DerivedTopNode	is a DerivedField
			type is like FinanceDimension1
			restricted
			return FinanceDimension1Structure.DimensionNode				
		SummaryNodeString	    is a StringField
			type is AlphaUpper 15
			restricted
			FinanceDimension1Structure.StructureSequence
			"_"
			NewDimension
		ParentNodeString	    is a StringField
			type is AlphaUpper 15
			restricted
			FinanceDimension1Structure.StructureSequence
			"_"
			NewDimension			
		LikeSummaryPrefix		is a StringField
			type is AlphaUpper 15
			restricted
			FinanceDimension1Structure.StructureSequence
			"_*"			
		DerivedParentDisplayDimension is a DerivedField
			type is like FinanceDimension1
			LocalParentFinanceDimension1 = DimensionInContextStructureRel.ParentDimension
			if (LocalParentFinanceDimension1 != DerivedTopNode)
				return LocalParentFinanceDimension1.DisplayDimension
		DerivedParentDescription is a DerivedField
			type is Alpha 60
			LocalParentFinanceDimension1 = DimensionInContextStructureRel.ParentDimension
			if (LocalParentFinanceDimension1 != DerivedTopNode)
				return LocalParentFinanceDimension1.Description				
		DescendantOfContextDimension is a DerivedField
			type is Boolean
			if (DescendantOfContextDimensionRel exists)
				return true
		ActorSecurityGroup is a DerivedField
			type is like FinanceDimension1SecurityGroup
			return actor.context.FinanceDimension1SecurityGroup


#ifdef module integration
		DerivedDelimiter is a DerivedField
			type is Alpha size 5
			restricted
			LocalConfigurationParameter = "Generic_Delimiter"
