						invoked.FinanceEnterpriseGroup		= FinanceEnterpriseGroup
						invoked.FinanceDimension1Structure	= FinanceDimension1Structure
						invoked.ParentDimension 			= ParentDimension 
						invoked.FinanceDimension1		    = FinanceDimension1
						invoked.DisplayDimension			= DisplayDimension
				    	invoked.SystemDimension				= SystemDimension	
				    	invoked.IsAZoneDimension			= IsAZoneDimension
				    	invoked.ZoneDimension				= ZoneDimension
				    	invoked.EnterpriseZoneDimension 	= EnterpriseZoneDimension
			    		invoked.DefaultZoneDimension		= DefaultZoneDimension
			    		invoked.IncludeInBudgetEdit         = IncludeInBudgetEdit 							
			
				if (!IsTopNode)
