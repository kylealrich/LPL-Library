		PurgeAssetHistory is a Set Action
			restricted

			Parameters
				PrmFromDate				is Date
					default label is "FromDate"
				PrmToDate				is Date
					default label is "ToDate"
				PrmCompany				is an AssetCompany
					default label is "Company"
				PrmCompanyGroup			is a GeneralLedgerCompanyGroup
					default label is "CompanyGroup"
				PrmAssetGroup			is an AssetGroup
					default label is "AssetGroup"
				PrmAssetType			is an AssetType
					default label is "AssetType"
				PrmAssetSubType			is an AssetType
					default label is "SubType"
				PrmAdjustmentHistory	is Boolean
					default label is "AdjustmentHistory"
				PrmTransferHistory		is Boolean
					default label is "TransferHistory"
				PrmDisposalHistory		is Boolean
					default label is "DisposalHistory"
				PrmReinstateHistory		is Boolean
					default label is "ReinstateHistory"
				PrmTransactions			is Boolean
					default label is "Transactions"
				PrmComments				is Boolean
					default label is "Comments"
				PrmUrls					is Boolean
					default label is "U\R\Ls"
				PrmRepairs				is Boolean
					default label is "Repairs"
				PrmImport				is Boolean
					default label is "Import"
				PrmSimulated			is AlphaUpper 1
					default label is "Simulated"
					States
						No		value is "N"
						Yes		value is "Y"
						Only	value is "O"

			Local Fields
				LocalAsyncId				 is an AsyncActionRequest
				LocalPurgeSimulated			 is Boolean
				LocalActor					 is an Actor
				LocalNotification			 is LPLText
				LocalNewLine				 is LPLText

				LocalSetAdjustmentCount		 is Numeric 10
				LocalSetTransferCount		 is Numeric 10
				LocalSetDisposalCount		 is Numeric 10
				LocalSetReinstateCount		 is Numeric 10
				LocalSetTransactionCount	 is Numeric 10
				LocalSetBookTransactionCount is Numeric 10
				LocalSetCommentCount		 is Numeric 10
				LocalSetRepairCount			 is Numeric 10
				LocalSetImportCount			 is Numeric 10

			Parameter Rules
				PrmFromDate
					if (PrmToDate entered)
						required
							"From_-_DateRequiredWhen_To_-_DateEntered"

					constraint (PrmFromDate <= PrmToDate)
						"InvalidDateRange"

				PrmCompany
					if (PrmCompanyGroup not entered)
						required
							"MustEnterEitherCompanyOrCompanyGroup"

					if (PrmCompany entered)
						constraint (PrmCompany.GeneralLedgerCompany.FinanceEnterpriseGroup = actor.context.FinanceEnterpriseGroup)
							"CanOnlyEnterCompaniesAssociatedWith_Finance_Enterprise_Group<actor.context.FinanceEnterpriseGroup>"

						constraint (PrmCompanyGroup not entered)
							"CannotEnterBothCompanyAndCompanyGroup"

				PrmCompanyGroup
					if (PrmCompany not entered)
						required
							"MustEnterEitherCompanyOrCompanyGroup"

					if (PrmCompanyGroup entered)
						constraint (first PrmCompanyGroup.GeneralLedgerCompanyGroupMembersRel.Company.FinanceEnterpriseGroup = actor.context.FinanceEnterpriseGroup)
							"CanOnlyEnterCompanyGroupsWith_Finance_Enterprise_Group<actor.context.FinanceEnterpriseGroup>"

						constraint (PrmCompany not entered)
							"CannotEnterBothCompanyAndCompanyGroup"
						LocalCompanyGroup	= PrmCompanyGroup

				PrmSimulated
					initial value is "N"
					default to "N"

					if (not PrmAdjustmentHistory
					and not PrmTransferHistory
					and not PrmDisposalHistory
					and not PrmReinstateHistory
					and not PrmTransactions
					and not PrmComments
					and not PrmUrls
					and not PrmRepairs
					and not PrmImport)
						constraint (PrmSimulated.Only)
							"MustSelectAtLeastOneFileToProcess"

			Instance Selection
				include deleted records
				where ((PrmCompany entered and Company = PrmCompany)
				or    CompanyGroupRel exists)

			Action Rules
				Set Rules
					Entrance Rules
						LocalAsyncId = current async action request id

				Empty Set Rules
					LocalActor = actor

						send notification
							to LocalActor
							description is "NoRecordsFoundToPurge"
							priority is high
							detail is "ThereAreNoAssetHistoryRecordsPurge"

				Instance Rules

					if (PrmSimulated.Yes or PrmSimulated.Only)
						LocalSimulated = true

					LocalFromDate				= PrmFromDate
					LocalToDate					= PrmToDate
					LocalAssetGroup				= PrmAssetGroup
					LocalAssetType				= PrmAssetType
					LocalAssetSubType			= PrmAssetSubType
					LocalPurgeDisposalHistory	= PrmDisposalHistory

					if  (PrmAdjustmentHistory)
						initialize LocalSetAdjustmentCount
						LocalSetAdjustmentCount += instance count of PurgeAssetAdjustmentRel
						invoke PurgeAssetAdjustments AssetAdjustment
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

						invoke PurgeDeletedAssetAdjustments AssetAdjustment
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

					if  (PrmTransferHistory)
						initialize LocalSetTransferCount
						LocalSetTransferCount += instance count of PurgeAssetTransferRel
						invoke PurgeAssetTransfers AssetTransfer
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

						invoke PurgeDeletedAssetTransfers AssetTransfer
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

					if  (PrmDisposalHistory)
						initialize LocalSetDisposalCount
						LocalSetDisposalCount += instance count of PurgeAssetDisposalRel
						invoke PurgeAssetDisposals AssetDisposal
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

						invoke PurgeDeletedAssetDisposals AssetDisposal
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmPurgeSimulated	= LocalSimulated
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

					if (PrmReinstateHistory)
						initialize LocalSetReinstateCount
						LocalSetReinstateCount += instance count of PurgeAssetReinstatementRel
						invoke PurgeAssetReinstatements AssetReinstatement
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

						invoke PurgeDeletedAssetReinstatements AssetReinstatement
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmCompany			= Company
							invoked.PrmFromDate			= PrmFromDate
							invoked.PrmToDate			= PrmToDate
							invoked.PrmAssetGroup		= PrmAssetGroup
							invoked.PrmAssetType		= PrmAssetType
							invoked.PrmAssetSubType		= PrmAssetSubType

					if (PrmTransactions)
						initialize LocalSetTransactionCount
						LocalSetTransactionCount += instance count of PurgeAssetTransactionRel
						invoke PurgeAssetTransactions AssetTransaction
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmCompany		= Company
							invoked.PrmFromDate		= PrmFromDate
							invoked.PrmToDate		= PrmToDate
							invoked.PrmAssetGroup	= PrmAssetGroup
							invoked.PrmAssetType	= PrmAssetType
							invoked.PrmAssetSubType	= PrmAssetSubType

						initialize LocalSetBookTransactionCount
						LocalSetBookTransactionCount += instance count of PurgeAssetBookTransactionRel
						invoke PurgeAssetBookTransactions AssetBookTransaction
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmCompany		= Company
							invoked.PrmFromDate		= PrmFromDate
							invoked.PrmToDate		= PrmToDate
							invoked.PrmAssetGroup	= PrmAssetGroup
							invoked.PrmAssetType	= PrmAssetType
							invoked.PrmAssetSubType	= PrmAssetSubType

					if (PrmComments)
						initialize LocalSetCommentCount
						LocalSetCommentCount += instance count of PurgeAssetCommentRel
						invoke PurgeAssetComments AssetComment
							run after LocalAsyncId
							assign async action request id to LocalAsyncId
							invoked.PrmCompany = Company

					if  (PrmRepairs)
						initialize LocalSetRepairCount
						LocalSetRepairCount += instance count of PurgeAssetRepairRel
