	Conditions
		PeriodCloseReady
			when (AMSystemClosingControlRel.Control	= true
			and  AMSystemClosingControlRel.CurrentPeriodNotClosed
			and  DepreciationCalculated = blank)

		AssetPeriodClosed
			when (!(AMSystemClosingControlRel.CurrentPeriodNotClosed
			and AMSystemClosingControlRel.Control = true))

		YearEndInProgress
			when (PendingYearEndCount > 0)

		DepreciationCalculated
			when (instance count of CloseBookCalendarsRel = instance count of CalculatedCloseBookCalendarsRel)

		SecurityGroupAllowsAccess
			when (actor.context.CompanySecurityGroup = blank
			or    CompanySecurityGroupMemberRel exists)

		AssetAdjustmentExists
			restricted
			classic name is ADJUST-EXIST
			when (first AssetAdjustmentsRel exists)

		UnreleasedAssetExists
			classic name is ASSET1-EXIST
			when (first UnreleasedAssetsRel exists)


		ReleasedAssetsExist
			restricted

			when (first ReleasedAssetsRel exists)

		PostReleaseAssetExists
			classic name is ASSET3-EXIST
			when (first PostReleaseAssetsRel exists)

		Released
			when (!PostReleaseAssetExists
			and  !UnreleasedAssetExists
			and PeriodCloseRelease != "N")

		AssetAccountingUnitGroupEntered
			restricted
			classic name is AUGRP-EXIST
			when (first AssetAccountingUnitGroupsRel exists)

		IsCalExist
			restricted
			when (first BookCalendarsRel exists)

		CompanyEntered
			restricted
			classic name is COMPANY-EXIST
			when (Company entered)





		IsGrpExist
			restricted
			when (first AssetAccountGroupsRel exists)

		IsOptionsExist
			restricted
			when (first AssetSystemOptionsRel exists)





		AssetTransferExists
			restricted
			classic name is TRF-EXIST
			when (first AssetTransfersRel exists)

		IsTypeExist
			restricted
			when (first AssetTypesRel exists)

	   	SystemDepreciationMethodsExist
	   		restricted
	   		when (MethodRel exists)

	   	IsRevalueCompany
	   		restricted
	   		when (Revalue)

		CloseControlIsActive
			when (AMSystemClosingControlRel.Control)

		CloseControlExists
			when (AMSystemClosingControlRel exists)

		AssetsExist
			restricted
			when (ByAssetDescendingRel exists)

		IsCompanyAsset
			restricted
			when (TransactionReference.CompanyAsset)

		AssetInterfaceRecordsExist
			when (AssetInterface set exists)

		AssetImportRecordsExist
			when (AssetImport set exists)

		AssetGuidelineClassesExist
			restricted
			when (AssetGuidelineClassesRel exists)

		IsBalancingTransfers
			restricted
			when (BalanceTransfers.Yes)

		FundAccounting
			restricted
			when (FinanceEnterpriseGroup.FundAccounting)

		LocalEventCodeRelExists
			restricted
			when (LocalEventCodeRel exists)

		IsValidForActorContext
			restricted
			when (GeneralLedgerCompanyRel.FinanceEnterpriseGroup = actor.context.FinanceEnterpriseGroup)

	Relations
		AssetBatchUpdateResultRel
			one-to-many relation to AssetBatchUpdateResult
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup		= FinanceEnterpriseGroup

		AssetGuidelineClassesRel
			one-to-many relation to AssetGuidelineClass
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup		= FinanceEnterpriseGroup

		SecurityGroupRel
			one-to-one relation to FinanceDimensionSecurityGroup
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup 		  = FinanceEnterpriseGroup
				related.DimensionGroupType	   		  = 2
				related.FinanceDimensionSecurityGroup = ActorSecurityGroup

		SecurityGroupDetailRel
			one-to-one relation to GeneralLedgerCompanyGroupMember
			Field Mapping uses Set1
				related.Company						= Company
				related.GeneralLedgerCompanyGroup  	= SecurityGroupRel.GeneralLedgerCompanyGroup

 		CompanySecurityGroupMemberRel
			one-to-one relation to GeneralLedgerCompanyGroupMember
			Field Mapping uses symbolic key
				related.GeneralLedgerCompanyGroup	= actor.context.CompanySecurityGroup.FinanceDimensionStructure
				related.Company                     = Company

		AssetAccountGroupsRel
			classic name is AMACCTGRP
			one-to-many relation to AssetAccountGroup
			delete cascades
			Field Mapping uses symbolic key
				related.Company 					= Company

		CompanyGroupRel
			one-to-one relation to GeneralLedgerCompanyGroupMember
			Field Mapping uses symbolic key
				related.GeneralLedgerCompanyGroup 	= LocalCompanyGroup
				related.Company						= Company

		UnreleasedAssetsRel
			classic name is AMASSET1
			one-to-many relation to Asset
			Field Mapping uses Set9
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Status  					= "1"
				related.Company 					= Company
			Instance Selection
				where related.WorkInProcess = false
		AssetsSelectedForReleaseRel
			one-to-many relation to Asset
			Field Mapping uses Set9
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Status  					= "1"
				related.Company 					= LocalAssetCompany
			Instance Selection
				where ((related.AssetProcess			= LocalProcessType
				or LocalProcessType not entered)
				and (related.AssetLocation				= LocalLocation
				or LocalLocation not entered)
				and (related.AssetDivision				= LocalDivision
				or LocalDivision not entered)
				and (related.AssetGroup					= LocalAssetGroup
				or LocalAssetGroup not entered)
				and (related.AssetAccountingUnit		= LocalDefaultAU
				or LocalDefaultAU not entered)
				and (related.AssetAccountingUnitGroup	= LocalAUGroup
				or LocalAUGroup not entered)
				and (related.AssetType	 				= LocalAssetType
				or LocalAssetType not entered)
				and (related.CreatedBy					= LocalCreatedBy
				or LocalCreatedBy not entered))

		PostRelAssetsSelectedForReleaseRel
			one-to-many relation to Asset
			Field Mapping uses Set9
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Status  					= "3"
				related.Company 					= LocalAssetCompany
			Instance Selection
				where ((related.AssetProcess			= LocalProcessType
				or LocalProcessType not entered)
				and (related.AssetLocation				= LocalLocation
				or LocalLocation not entered)
				and (related.AssetDivision				= LocalDivision
				or LocalDivision not entered)
				and (related.AssetGroup					= LocalAssetGroup
				or LocalAssetGroup not entered)
				and (related.AssetAccountingUnit		= LocalDefaultAU
				or LocalDefaultAU not entered)
				and (related.AssetAccountingUnitGroup	= LocalAUGroup
				or LocalAUGroup not entered)
				and (related.AssetType	 				= LocalAssetType
				or LocalAssetType not entered)
				and (related.CreatedBy					= LocalCreatedBy
				or LocalCreatedBy not entered))

		ReleasedAssetsRel
			classic name is AMASSET2
			one-to-many relation to Asset
			Field Mapping uses Set9
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Status  					= 2
				related.Company 					= Company

		PostReleaseAssetsRel
			classic name is AMASSET3
			one-to-many relation to Asset
			Field Mapping uses Set9
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Status  					= 3
				related.Company 					= Company

		ReleasedPostReleasedAndDisposedAssets
			one-to-many relation to Asset
			Field Mapping uses ByCompanyAssetDescending
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Company 					= Company
			Instance Selection
				where (!related.Status.Unreleased)

		AssetsByCompanyAndAssetRel
			one-to-many relation to Asset
			Field Mapping uses Set2
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Company 					= Company
			Instance Selection
				where (!related.Status.Unreleased)

		AssetsByCompanyAssetRel
			one-to-many relation to Asset
			Field Mapping uses ByCompanyAsset
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Company 					= Company
			Instance Selection
				where (!related.Status.Unreleased)

		ByAssetDescendingRel
			one-to-many relation to Asset
			Field Mapping uses ByAssetDescending
				related.FinanceEnterpriseGroup		= Company.AccountingEntity.FinanceEnterpriseGroup

		LastAssetRel
			one-to-one relation to Asset
			Field Mapping uses ByAssetDescending
				related.FinanceEnterpriseGroup		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Asset                       = DerivedLastAsset

		ApprovalAssetsRel
			classic name is AMASSET2
			one-to-many relation to Asset
			Field Mapping uses ByCompanyAsset
			  	related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Company 					= Company
			Instance Selection
				where (related.ApprovalStatus  			= "S")

		AssetAdjustmentsRel
			classic name is AMASSETADJ
			one-to-many relation to AssetAdjustment
			Field Mapping uses Set2
				related.Company = Company

		AssetLocationsRel
			one-to-many relation to AssetLocation
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup

		AssetGroupsRel
			one-to-many relation to AssetGroup
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup

		AssetDivisionsRel
			one-to-many relation to AssetDivision
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup 		= Company.AccountingEntity.FinanceEnterpriseGroup

	  	GeneralLedgerSystemCodeRel
			one-to-one relation to GeneralLedgerSystemCode
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup      = Company.AccountingEntity.FinanceEnterpriseGroup
				related.GeneralLedgerSystemCode    	= "AM"

		AssetTemplatesRel is an AssetTemplate set

		AssetTransfersRel
			classic name is AMASSETTRF
			one-to-many relation to AssetTransfer
			Field Mapping uses Set2
				related.Company 					= Company

		AssetTypesRel
			classic name is AMASTTYPE
			one-to-many relation to AssetType
			delete cascades
			Field Mapping uses symbolic key
				related.Company 					= Company

		AssetTypesNoSubtypesRel
			classic name is AMASTTYPE
			one-to-many relation to AssetType
			Field Mapping uses ByAssetTypeSubType
				related.Company 					= Company
			Instance Selection
				where (related.AssetType.SubType = blank)

		AssetAccountingUnitGroupsRel
			classic name is AMAUGROUP
			one-to-many relation to AssetAccountingUnitGroup
			delete cascades
			Field Mapping uses symbolic key
				related.Company 					= Company

		BookCalendarsRel
			classic name is AMCALENDAR
			one-to-many relation to BookCalendar
			Field Mapping uses Set4
				related.Company 					= Company

		CalculatedCloseBookCalendarsRel
			one-to-many relation to BookCalendar
			Field Mapping uses BookCalendarsByStatus
				related.Company 					= Company
				related.CalendarStatus				= "C"
			Instance Selection
				where ((related.CloseBook = 0
				and related.PostingBook = true)
				and (related.DepreciationCalculated
				or related.ClosedForCurrentEntityPeriod))

		CloseBookCalendarsRel
			one-to-many relation to BookCalendar
			Field Mapping uses BookCalendarsByStatus
				related.Company 					= Company
				related.CalendarStatus				= "C"
			Instance Selection
				where (related.CloseBook = 0
				and related.PostingBook = true)

		BookCalendarForCompanyRel
			one-to-many relation to BookCalendar
			Field Mapping uses ByCompanyBookStatus
				related.FinanceEnterpriseGroup		= Company.AccountingEntity.FinanceEnterpriseGroup
				related.Company 					= Company

		PostingBookCalendarRel
			one-to-many relation to BookCalendar
			Field Mapping uses CompanyHistoryPostingBooks
				related.Company						= Company
				related.PostingBook					= true

		AssetSystemOptionsRel
			classic name is AMSYSOPTS
			one-to-many relation to AssetSystemOption
			Field Mapping uses symbolic key
				related.Company 					= Company

		AssetTransactionsRel
			classic name is AMTRANS
			one-to-many relation to AssetTransaction
			Field Mapping uses Set2
				related.Company                     = Company
				related.TransactionStatus			= "R"

		CompanyAssetTransactionsRel
