			Local Fields
				AssetCount is like Asset
			Action Rules
				AssetCount = 0
				for each AssetsByCompanyAndAssetRel
					AssetCount += 1
					if (each.Asset.Status = 2)
						invoke Released.UpdateReleasedSequenceNumbers each
							invoked.CompanyAsset = AssetCount
					if (each.Asset.Status = 3)
						invoke PostRelease.UpdatePostReleasedSequenceNumbers each
							invoked.CompanyAsset = AssetCount
					if (each.Asset.Status = 9)
						invoke Disposed.UpdateDisposedSequenceNumbers each
							invoked.CompanyAsset = AssetCount
				LastCompanyAsset = instance count of AssetsByCompanyAndAssetRel

		UpdateTransferBalancing is an Instance Action
			valid when (FundAccounting)
			Parameters
				PrmBalanceTransfers			is Numeric 1
					default label is "BalanceTransfers"
					States
						TurnOff						value is 0
						TurnOn						value is 1
				UpdateBalancingAccounts		is Numeric 1
					States
						No						value is 0
						AllTypesAndSubtypes		value is 1
						TypesOnly 				value is 2
				TransferOutAccount 			is a GeneralLedgerChartAccount
				TransferInAccount 			is a GeneralLedgerChartAccount
			Parameter Rules
				PrmBalanceTransfers
					if (PrmBalanceTransfers.TurnOn)
						constraint (TransferBalancingEventCodeExists)
							"PleaseRunUpdateEventCodesOnTheFinanceEnterpriseGroupPriorToEnablingTransferBalancing"
						if (UpdateBalancingAccounts.No)
							for each AssetTypesNoSubtypesRel
								LocalAssetType	= each.AssetType.Type
								LocalATO  		= each.ATOTransferAccount
								LocalATI 		= each.ATITransferAccount
								display "LocalATO<LocalATO>"
								display "LocalATI<LocalATI>"
								display "AC_2460LocalAssetType<LocalAssetType>"
								constraint (LocalATO entered)
									"WhenUpdateBalancingAccountsIsNotSelected,BalanceTransfersRequiresTransferAccountsToBeDefinedOnAllAssetTypesForTheCompany,FirstAssetTypeInErrorIs:<LocalAssetType>"
								constraint (LocalATI entered)
									"WhenUpdateBalancingAccountsIsNotSelected,BalanceTransfersRequiresTransferAccountsToBeDefinedOnAllAssetTypesForTheCompany,FirstAssetTypeInErrorIs:<LocalAssetType>"
						else
							constraint (TransferOutAccount entered)
								"TransferInAndOutAccountsAreRequired"
							constraint (TransferInAccount entered)
								"TransferInAndOutAccountsAreRequired"
				UpdateBalancingAccounts
					if (UpdateBalancingAccounts.No)
						constraint (TransferOutAccount not entered)
							"TransferAccountsCannotBeProvidedWhenNotUpdatingAccounts"
						constraint (TransferInAccount not entered)
							"TransferAccountsCannotBeProvidedWhenNotUpdatingAccounts"
			Action Rules
				if (UpdateBalancingAccounts.AllTypesAndSubtypes)
					for each AssetTypesRel
						invoke BalanceTransfersUpdate each
							display "AC_2460TransferOut<TransferOutAccount>"
							display "AC_2460TransferIn<TransferInAccount>"
							invoked.TransferOutAccount 	= TransferOutAccount
							invoked.TransferInAccount	= TransferInAccount
				if (UpdateBalancingAccounts.TypesOnly)
					for each AssetTypesNoSubtypesRel
						invoke BalanceTransfersUpdate each
							display "AC_2465TransferOut<TransferOutAccount>"
							display "AC_2465TransferIn<TransferInAccount>"
							invoked.TransferOutAccount 	= TransferOutAccount
							invoked.TransferInAccount	= TransferInAccount
				if (PrmBalanceTransfers.TurnOn)
					BalanceTransfers = true
				else
					BalanceTransfers = false

