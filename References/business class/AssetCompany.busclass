	Field Rules
		PeriodCloseRelease
			initial value is "Y"

		ItemNumbering
			initial value is ItemNumbering.Tag

		DefaultDepreciationExpenseProject
			initial value is true

		ConsolidateDisposalJournalEntry
			initial value is true

	 	DisplayJournalEntries
			initial value is true

		PayablesDistributionWithTax
			initial value is PayablesDistributionWithTax.Detail
		Revalue
			if (Revalue = true)
				for each AssetAccountingUnitGroup set
					constraint (each.RevalueSurplusAccountingUnit entered
					and each.RevalueLossAccountingUnit entered)
						"AssetDimensionGroup<each.AssetAccountingUnitGroup>MustBeAssignedRevalueDimensions"
				for each AssetAccountGroup set
					constraint (each.AssetAccountRVSRel.DistributionAccount entered)
						"AssetAccountGroup<each.AssetAccountGroup>MustBeAssignedRevalueAccounts"
					constraint (each.AssetAccountRVLRel.DistributionAccount entered)
						"AssetAccountGroup<each.AssetAccountGroup>MustBeAssignedRevalueAccounts"
		NeedsApproval
			if (NeedsApproval changed
			and !NeedsApproval)
				constraint (ApprovalAssetsRel not exists)
					"WaitingForApprovalAssetsExists;CannotChange"
		ApprovalCode
			if(DepreciationApproval)
				required
					"ApprovalCodeMustBeEntered;DepreciationApprovalEnabled"


	Rule Blocks
		VerifyTagsAreUnique
			initialize LocalTagNumber
			initialize LocalAsset
			for each CompanyAssetsByTagNumberRel
				display "AC_913LocalTagNumber<LocalTagNumber>"
				if (each.TagNumber entered)
					display "AC_915LocalTagNumber<LocalTagNumber>"
					if (LocalTagNumber entered)
						display "AC_917LocalTagNumber<LocalTagNumber>"
						if (LocalTagNumber != each.TagNumber)
							display "AC_919LocalTagNumber<LocalTagNumber>"
							LocalTagNumber	= each.TagNumber
							LocalAsset		= each.Asset
							display "AC_921LocalTagNumber<LocalTagNumber>"
						else
							display "AC_923LocalTagNumber<LocalTagNumber>"
							LocalNewAsset	= each.Asset
							constraint (LocalTagNumber != each.TagNumber)
								"CannotRequireUniqueTagNumbers:TagNumber<LocalTagNumber>IsAssignedToAsset<LocalAsset>And<LocalNewAsset>"
					else
						LocalTagNumber		= each.TagNumber
						LocalAsset			= each.Asset



