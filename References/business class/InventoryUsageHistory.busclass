	Rule Blocks
		SupplementalQuantityComputation
			LocalSupplementalQuantityComputed 				= true
			LocalSupplementalUOMMultiplier					= DerivedSupplementalUOMMultiplier
			initialize UOMCalculation
			
			initialize Counter
			while (Counter < 14)
				Counter +=1

				LocalReceiptArrayInSupplementalUOM.InventoryReceipts[Counter].ReceiptQty = (InventoryReceiptsArray.InventoryReceipts[Counter].ReceiptQty / LocalSupplementalUOMMultiplier)
				LocalTotalReceiptQuantityInSupplementalUOM += LocalReceiptArrayInSupplementalUOM.InventoryReceipts[Counter].ReceiptQty

				LocalAdjustmentArrayInSupplementalUOM.InventoryAdjustments[Counter].AdjustmentInQuantity = ((InventoryAdjustmentsArray.InventoryAdjustments[Counter].AdjustmentInQuantity - InventoryAdjustmentsArray.InventoryAdjustments[Counter].AdjustmentOutQuantity) / LocalSupplementalUOMMultiplier)		
				LocalTotalAdjustmentQuantityInSupplementalUOM += LocalAdjustmentArrayInSupplementalUOM.InventoryAdjustments[Counter].AdjustmentInQuantity

				LocalInventoryTransfersArrayInSupplementalUOM.InventoryTransfers[Counter].TransferInQuantity = ((InventoryTransfersArray.InventoryTransfers[Counter].TransferInQuantity - InventoryTransfersArray.InventoryTransfers[Counter].TransferOutQuantity) / LocalSupplementalUOMMultiplier)		
				LocalTotalTransferQuantityInSupplementalUOM += LocalInventoryTransfersArrayInSupplementalUOM.InventoryTransfers[Counter].TransferInQuantity

				LocalIssueAndSalesArrayInSupplementalUOM.InventoryIssues[Counter].IssueQuantity = ((InventoryIssuesArray.InventoryIssues[Counter].IssueQuantity + InventorySalesArray.InventorySales[Counter].SalesQuantity) / LocalSupplementalUOMMultiplier)		
				LocalTotalIssueAndSalesQuantityInSupplementalUOM += LocalIssueAndSalesArrayInSupplementalUOM.InventoryIssues[Counter].IssueQuantity

		LocationUOMQuantityComputation
			LocalLocationStockQuantityComputed 				= true
			LocalLocationStockUOMMultiplier					= DerivedLocationStockUOMMultiplier
			
			initialize Counter
			while (Counter < 14)
				Counter +=1

				LocalReceiptArrayInLocationStockUOM.InventoryReceipts[Counter].ReceiptQty = (InventoryReceiptsArray.InventoryReceipts[Counter].ReceiptQty / LocalLocationStockUOMMultiplier)
				LocalTotalReceiptQuantityInLocationStockUOM += LocalReceiptArrayInLocationStockUOM.InventoryReceipts[Counter].ReceiptQty

				LocalAdjustmentArrayInLocationStockUOM.InventoryAdjustments[Counter].AdjustmentInQuantity = ((InventoryAdjustmentsArray.InventoryAdjustments[Counter].AdjustmentInQuantity - InventoryAdjustmentsArray.InventoryAdjustments[Counter].AdjustmentOutQuantity) / LocalLocationStockUOMMultiplier)		
				LocalTotalAdjustmentQuantityInLocationStockUOM += LocalAdjustmentArrayInLocationStockUOM.InventoryAdjustments[Counter].AdjustmentInQuantity

				LocalInventoryTransfersArrayInLocationStockUOM.InventoryTransfers[Counter].TransferInQuantity = ((InventoryTransfersArray.InventoryTransfers[Counter].TransferInQuantity - InventoryTransfersArray.InventoryTransfers[Counter].TransferOutQuantity) / LocalLocationStockUOMMultiplier)		
				LocalTotalTransferQuantityInLocationStockUOM += LocalInventoryTransfersArrayInLocationStockUOM.InventoryTransfers[Counter].TransferInQuantity

				LocalIssueAndSalesArrayInLocationStockUOM.InventoryIssues[Counter].IssueQuantity = ((InventoryIssuesArray.InventoryIssues[Counter].IssueQuantity + InventorySalesArray.InventorySales[Counter].SalesQuantity) / LocalLocationStockUOMMultiplier)		
				LocalTotalIssueAndSalesQuantityInLocationStockUOM += LocalIssueAndSalesArrayInLocationStockUOM.InventoryIssues[Counter].IssueQuantity


