						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension9Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<FinanceDimension9.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension9Label>"	
					TopNodeFound = false
