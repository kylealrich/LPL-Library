		RequestNewFinanceDimension9 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
			    FinanceDimension9Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension9Label>Structure"
				DisplayDimension 				is like FinanceDimension9
					translatable
				PrmFinanceDimension9			is like FinanceDimension9
				ParentFinanceDimension9			is a FinanceDimension9
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension9Structure
					initial value is FinanceEnterpriseGroup.EnterpriseDim9Structure
					default to FinanceEnterpriseGroup.EnterpriseDim9Structure
				DisplayDimension
					default to PrmFinanceDimension9
				PrmFinanceDimension9
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension9Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension9RegularExpression entered
					and CreateDimensionType.Posting)
						constraint (PrmFinanceDimension9 matches FinanceEnterpriseGroup.FinanceDimension9RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension9Label>"
					LocalStructure 					= FinanceDimension9Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= PrmFinanceDimension9
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension9>"
				CreateDimensionType		
					required
				BudgetIdentifier
					constraint (CreateDimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (CreateDimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension9Label>"	
					constraint (FinanceDimension9Structure.DerivedSystemDimension = FinanceDimension9Structure.UndefinedMsg
						or FinanceDimension9Structure.DerivedSystemDimension = PrmFinanceDimension9)
						"<FinanceDimension9Structure.DerivedSystemDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension9Label>"
				Description
					required
				Active
					initial value is true
					if (SystemDimension)
						Active = true
			
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension9			= PrmFinanceDimension9
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension9
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension9 entered)
						invoked.ParentDimension			= ParentFinanceDimension9
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
