							constraint (FinanceDimension9 != LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension)
								"<NewParent>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension9 not entered)
