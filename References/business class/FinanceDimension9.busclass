							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
