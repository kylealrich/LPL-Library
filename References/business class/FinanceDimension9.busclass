								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension9 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension9 = LocalFinanceDimension9.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
					invoke NotifyMoveDimension9 NotifyBudgetTemplateRel
						invoked.PrmFinanceDimension9 = FinanceDimension9
						invoked.PrmNewParent		 = NewParent
						invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
