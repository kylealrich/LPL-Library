            Set Is
                PrmFinanceEnterpriseGroup
                PrmFinanceDimension1
                PrmProject
                PrmDefaultToValue
                PrmCodeBlockRelation
                PrmUniqueID
                                
            Local Fields
            	AccountCounter					is Numeric 5
            	SequenceCounter					is Numeric 5
				LocalFinanceDimension1Array		is a FinanceDimension1Array
            
            Instance Selection
                where (FinanceEnterpriseGroup = PrmFinanceEnterpriseGroup
                and    SummaryFinanceDimension1 = PrmFinanceDimension1
                and    FinanceDimension1.Active
                and    DimensionType.Posting)
            
            Sort Order
                FinanceEnterpriseGroup
                SummaryFinanceDimension1
                    
            Action Rules
                Set Rules
					Exit Rules
						if (AccountCounter > 0)
							invoke CreateFinanceDimension1StructureCodeBlockDetails ProjectShadow
								invoked.PrmFinanceEnterpriseGroup       = PrmFinanceEnterpriseGroup
								invoked.PrmFinanceDimension1Array       = LocalFinanceDimension1Array
                            	invoked.PrmProject                      = PrmProject
                            	invoked.PrmDefaultToValue               = PrmDefaultToValue
                            	invoked.PrmCodeBlockRelation            = PrmCodeBlockRelation
                            	invoked.PrmSequenceNumber				= SequenceCounter
                            	invoked.PrmUniqueID						= PrmUniqueID
                            	invoked.PrmCodeBlockRelationRule		= PrmCodeBlockRelationRule
                            	invoked.PrmDeleteFlag					= PrmDeleteFlag
                        
                Instance Rules
					AccountCounter += 1
					SequenceCounter += 1
					if (AccountCounter < FinanceEnterpriseGroup.StructureRelationArraySize)
						LocalFinanceDimension1Array.FinanceDimension1[AccountCounter] = FinanceDimension1
					else
						invoke CreateFinanceDimension1StructureCodeBlockDetails ProjectShadow
							invoked.PrmFinanceEnterpriseGroup       = PrmFinanceEnterpriseGroup
							invoked.PrmFinanceDimension1Array       = LocalFinanceDimension1Array
                            invoked.PrmProject                      = PrmProject
                            invoked.PrmDefaultToValue               = PrmDefaultToValue
                            invoked.PrmCodeBlockRelation            = PrmCodeBlockRelation
                            invoked.PrmSequenceNumber				= SequenceCounter 
                            invoked.PrmUniqueID						= PrmUniqueID
                            invoked.PrmCodeBlockRelationRule		= PrmCodeBlockRelationRule
                            invoked.PrmDeleteFlag					= PrmDeleteFlag
						initialize LocalFinanceDimension1Array
						initialize AccountCounter

						AccountCounter = 1
						LocalFinanceDimension1Array.FinanceDimension1[AccountCounter] = FinanceDimension1

