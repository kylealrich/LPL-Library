		Move is an Instance Action
			valid when (SummaryOrPostingDimension)
			
			Parameters
				PrmContextDimension is a FinanceDimension10ContextValue
				NewParent 			is a FinanceDimension10
							
			Parameter Rules
				PrmContextDimension
					initial value is FinanceDimension10
				NewParent
					default to DerivedTopNode
					required
					constraint (NewParent != FinanceDimension10)
						"CannotMoveToSelf"
					constraint (NewParent.DimensionType.Summary
					or			NewParent.DimensionType.Node)
						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension10Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<NewParent.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension10Label>"	
					TopNodeFound = false
					LocalFinanceDimension10 = NewParent
					if (!NewParent.DimensionType.Node)
						while (!TopNodeFound)
							constraint (FinanceDimension10 != LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension)
								"<FinanceDimension10>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension10 not entered)
								TopNodeFound = true
							else
								LocalFinanceDimension10 = LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension	
#ifdef module cam
					constraint (CustodialDetailAmountRel not exists)
						"CannotMove;<FinanceEnterpriseGroup.FinanceDimension10Label>DetailSetupFor<DerivedParentDimension>"
#endif													
			Local Fields
				OldParent is like FinanceDimension10
																		
			Entrance Rules
				include CheckValidContext
							
				OldParent				 = DerivedParentDimension
				ParentDimension			 = NewParent

			Action Rules
				ParentDimension  	 = NewParent
				invoke Update DimensionInContextStructureRel
					invoked.ParentDimension		 	= NewParent

			Exit Rules
 				LocalFinanceDimension10 = FinanceDimension10
 							

				include CreateShadowForDimension		

				if (DimensionType.Summary)
					TopNodeFound = false
					while (!TopNodeFound)
						if (LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.DimensionType.Summary)
							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
							if (LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier entered)
								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension10 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension10 = LocalFinanceDimension10.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
					invoke NotifyMoveDimension10 NotifyBudgetTemplateRel
						invoked.PrmFinanceDimension10 = FinanceDimension10
						invoked.PrmNewParent		 = NewParent
						invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
