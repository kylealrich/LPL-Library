		RequestNewFinanceDimension10 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
			    FinanceDimension10Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension10Label>Structure"
				DisplayDimension 				is like FinanceDimension10
					translatable
				PrmFinanceDimension10			is like FinanceDimension10
				ParentFinanceDimension10		is a FinanceDimension10
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension10Structure
					initial value is FinanceEnterpriseGroup.EnterpriseDim10Structure
					default to FinanceEnterpriseGroup.EnterpriseDim10Structure
				DisplayDimension
					default to PrmFinanceDimension10
				PrmFinanceDimension10
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension10Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension10RegularExpression entered
					and CreateDimensionType.Posting)
						constraint (PrmFinanceDimension10 matches FinanceEnterpriseGroup.FinanceDimension10RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension10Label>"
					LocalStructure 					= FinanceDimension10Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= PrmFinanceDimension10
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension10>"
				CreateDimensionType		
					required
				BudgetIdentifier
					constraint (CreateDimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (CreateDimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension10Label>"	
					constraint (FinanceDimension10Structure.DerivedSystemDimension = FinanceDimension10Structure.UndefinedMsg
						or FinanceDimension10Structure.DerivedSystemDimension = PrmFinanceDimension10)
						"<FinanceDimension10Structure.DerivedSystemDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension10Label>"
				Description
					required
				Active
					initial value is true
					if (SystemDimension)
						Active = true
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension10			= PrmFinanceDimension10
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension10
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension10 entered)
						invoked.ParentDimension			= ParentFinanceDimension10
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
