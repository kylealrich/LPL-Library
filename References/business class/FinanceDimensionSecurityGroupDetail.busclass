			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup		= FinanceEnterpriseGroup
				related.Project						= FinanceDimensionSecurityGroupDetail
		ChartAccountRel
			one-to-one relation to GeneralLedgerChartAccount
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup		= FinanceEnterpriseGroup
				related.GeneralLedgerChartAccount	= FinanceDimensionSecurityGroupDetail		
	Sets
		
	Field Rules
				
	Actions
		Create is an Action
			restricted
			bypass field rules
			valid when (MemberDimension)
		
			Entrance Rules
				if (DimensionGroupType.FinanceDimension1)
					FinanceDimensionSecurityGroupDetail = FinanceDimension1
					DimensionType						= FinanceDimension1.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension2)
					FinanceDimensionSecurityGroupDetail = FinanceDimension2
					DimensionType						= FinanceDimension2.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension3)
					FinanceDimensionSecurityGroupDetail = FinanceDimension3
					DimensionType						= FinanceDimension3.DimensionType							
				else
				if (DimensionGroupType.FinanceDimension4)
					FinanceDimensionSecurityGroupDetail = FinanceDimension4
					DimensionType						= FinanceDimension4.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension5)
					FinanceDimensionSecurityGroupDetail = FinanceDimension5
					DimensionType						= FinanceDimension5.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension6)
					FinanceDimensionSecurityGroupDetail = FinanceDimension6	
					DimensionType						= FinanceDimension6.DimensionType
				else
				if (DimensionGroupType.FinanceDimension7)
					FinanceDimensionSecurityGroupDetail = FinanceDimension7
					DimensionType						= FinanceDimension7.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension8)
					FinanceDimensionSecurityGroupDetail = FinanceDimension8
					DimensionType						= FinanceDimension8.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension9)
					FinanceDimensionSecurityGroupDetail = FinanceDimension9
					DimensionType						= FinanceDimension9.DimensionType	
				else
				if (DimensionGroupType.FinanceDimension10)
					FinanceDimensionSecurityGroupDetail = FinanceDimension10
					DimensionType						= FinanceDimension10.DimensionType
				else
				if (DimensionGroupType.Project)
					FinanceDimensionSecurityGroupDetail = Project
					DimensionType						= Project.ProjectType
				else
				if (DimensionGroupType.ChartAccount)
					FinanceDimensionSecurityGroupDetail = GeneralLedgerChartAccount
					if (GeneralLedgerChartAccount.ChartType.Node)
						DimensionType					= DimensionType.Node
					else
					if (GeneralLedgerChartAccount.ChartType.Summary)
						DimensionType					= DimensionType.Summary
					else
						DimensionType					= DimensionType.Posting

		Delete is an Action
			restricted

		Purge is a Purge Action
			restricted

		ResetCreatedForHierarchyAccess is an Instance Action
			restricted
			
			Action Rules
				CreatedForHierarchyAccess = false
				
		DeleteDetailsForGroup is a Set Action
			restricted
			disable checkpoint
			
			Parameters
				PrmFinanceEnterpriseGroup is a FinanceEnterpriseGroup
					default label is "FinanceEnterpriseGroup"
				PrmDimensionGroupType	  is a DimensionGroupType
					default label is "DimensionGroupType"
				PrmFinanceDimensionSecurityGroup is a FinanceDimensionSecurityGroup
					default label is "FinanceDimensionSecurityGroup"
				PrmKeepFinanceDimensionSecurityGroup is Boolean
			
			Instance Selection
				where (FinanceEnterpriseGroup 		 = PrmFinanceEnterpriseGroup
				and    DimensionGroupType    		 = PrmDimensionGroupType
				and    FinanceDimensionSecurityGroup = PrmFinanceDimensionSecurityGroup)		
			
			Action Rules
				Set Rules
					Exit Rules
						if (!PrmKeepFinanceDimensionSecurityGroup)
							invoke Delete PrmFinanceDimensionSecurityGroup
				Instance Rules
					invoke Purge
							
		DeleteDetailsForMatrixGroup is a Set Action
			restricted
			disable checkpoint
			
			Parameters
				PrmFinanceEnterpriseGroup is a FinanceEnterpriseGroup
					default label is "FinanceEnterpriseGroup"
				PrmDimensionGroupType	  is a DimensionGroupType
					default label is "DimensionGroupType"
				PrmFinanceDimensionSecurityGroup is a FinanceDimensionSecurityGroup
					default label is "FinanceDimensionSecurityGroup"
				PrmKeepFinanceDimensionSecurityGroup is Boolean
			
			Instance Selection
				where (FinanceEnterpriseGroup 		 = PrmFinanceEnterpriseGroup
				and    DimensionGroupType    		 = PrmDimensionGroupType
				and    FinanceDimensionSecurityGroup = PrmFinanceDimensionSecurityGroup
				and   !MatrixSecurityDetail)
			
			Action Rules
				Set Rules
					Exit Rules
						if (!PrmKeepFinanceDimensionSecurityGroup)
							invoke Delete PrmFinanceDimensionSecurityGroup
				Instance Rules
					invoke Purge

		DeleteMatrixDetails is a Set Action
			restricted
			disable checkpoint
			
			Parameters
				PrmFinanceEnterpriseGroup is a FinanceEnterpriseGroup
					default label is "FinanceEnterpriseGroup"
			Instance Selection
				where (FinanceEnterpriseGroup = PrmFinanceEnterpriseGroup
				and    MatrixSecurityDetail)
			
			Action Rules
				Instance Rules
					invoke Purge
