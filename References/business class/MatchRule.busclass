			one-to-one relation to MatchMessageTolerance
			Field Mapping uses symbolic key
				related.FinanceEnterpriseGroup	= FinanceEnterpriseGroup
				related.MatchRule 				= MatchRule

	Conditions
		HasInvoiceMatchRuleGroup
			when (MatchRuleToleranceArray.MatchRuleToleranceGroup[1].InvoiceMatchLevel.InvoiceMatch
			or    MatchRuleToleranceArray.MatchRuleToleranceGroup[2].InvoiceMatchLevel.InvoiceMatch
			or    MatchRuleToleranceArray.MatchRuleToleranceGroup[3].InvoiceMatchLevel.InvoiceMatch
			or    MatchRuleToleranceArray.MatchRuleToleranceGroup[4].InvoiceMatchLevel.InvoiceMatch
			or    MatchRuleToleranceArray.MatchRuleToleranceGroup[5].InvoiceMatchLevel.InvoiceMatch
			or    MatchRuleToleranceArray.MatchRuleToleranceGroup[6].InvoiceMatchLevel.InvoiceMatch)

		Copy1To2Valid
		    when (MatchRuleToleranceArray.MatchRuleToleranceGroup[1].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[2].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[1].MatchRuleType entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[2].MatchRuleType entered)

		Copy2To3Valid
		    when (MatchRuleToleranceArray.MatchRuleToleranceGroup[2].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[3].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[2].MatchRuleType entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[3].MatchRuleType entered)

		Copy3To4Valid
		    when (MatchRuleToleranceArray.MatchRuleToleranceGroup[3].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[4].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[3].MatchRuleType entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[4].MatchRuleType entered)

		Copy4To5Valid
		    when (MatchRuleToleranceArray.MatchRuleToleranceGroup[4].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[5].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[4].MatchRuleType entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[5].MatchRuleType entered)

		Copy5To6Valid
		    when (MatchRuleToleranceArray.MatchRuleToleranceGroup[5].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[6].InvoiceMatchLevel entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[5].MatchRuleType entered
		    and   MatchRuleToleranceArray.MatchRuleToleranceGroup[6].MatchRuleType entered)

		MatchRuleExists
		    when (MatchRule exists)
		    
