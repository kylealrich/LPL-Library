		I2			is Numeric 5
		From        is Numeric 5
		To          is Numeric 5
		
	Field Rules
		Description
			required

		MatchRuleToleranceArray
			required
				"MustEnterAtLeastOneLevelOfMatching"    

	Rule Blocks
		SetOrder
			initialize I1
			while (I1 <= 6)
            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleType entered)
					if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].InvoiceMatchLevel entered)
						MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleOrder = I1
	
					if  (!MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateOpenMessageforUnderCost
					and  !MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateClosedMessageforUnderCost)
						MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateClosedMessageforUnderCost = true
	
					if  (!MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateOpenMessageforUnderQuantity
					and  !MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateClosedMessageforUnderQuantity)
						MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].CreateClosedMessageforUnderQuantity = true

				I1 = I1 + 1

		CheckContiguous
			I1 = 2
			while (I1 < 6)
				I2 = I1 - 1
            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleType entered)
            	    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[I2].MatchRuleType entered)
						"RuleTypeMustBeContiguous"
						
            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].InvoiceMatchLevel entered)
            	    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[I2].InvoiceMatchLevel entered)
						"MatchLevelMustBeContiguous"

            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchToleranceOption entered)
            	    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[I2].MatchToleranceOption entered)
						"ToleranceOptionMustBeContiguous"

				I1 = I1 + 1

		CheckMismatch
			I1 = 1
			while (I1 <= 6)

            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].InvoiceMatchLevel.InvoiceMatch 
				and (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleType.AllToOne
				or   MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleType.AllToAll))
					I2 = 1 
					while (I2 <= 6)
	            	    constraint (!MatchRuleToleranceArray.MatchRuleToleranceGroup[I2].InvoiceMatchLevel.DetailMatch)
							"CannotHaveDetailMatchLevelWithAnAllToOneOrAllToAllInvoiceMatchLevelInTheSameRuleGroup"
						I2 = I2 + 1

				I1 = I1 + 1

		CheckReset
			I1 = 1
			while (I1 <= 6)

            	if  (MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].InvoiceMatchLevel not entered
            	and  MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchRuleType not entered
            	and  MatchRuleToleranceArray.MatchRuleToleranceGroup[I1].MatchToleranceOption not entered)
            		initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[I1]

				I1 = I1 + 1

		CopyTolerances
		    display "CopyTolFrom<From>To<To>"
		    display "FromMatchUnderAmountTolerance:<MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountTolerance>"
		    display "ToMatchUnderAmountTolerance:<MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderAmountTolerance>"


		    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[From].InvoiceMatchLevel entered)
		        "FromMatchLevelNotEntered"
		    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[To].InvoiceMatchLevel entered)
		        "ToMatchLevelNotEntered"
		    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchRuleType entered)
		        "FromMatchTypeNotEntered"
		    constraint (MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchRuleType entered)
		        "ToMatchTypeNotEntered"

            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderAmountTolerance            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderAmountPercentage           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderAmountCondition            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverAmountTolerance             = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverAmountPercentage            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverAmountCondition             = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverQuantityTolerance           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverQuantityCondition           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchOverQuantityPercentage          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderQuantityTolerance          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderQuantityCondition          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderQuantityPercentage         = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].CreateChargeback                     = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateChargeback
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverAmountTolerance             = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverAmountCondition             = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverAmountPercentage            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderAmountTolerance            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderAmountCondition            = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderAmountPercentage           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].CreateOpenMessageforUnderCost        = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateOpenMessageforUnderCost
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].CreateClosedMessageforUnderCost      = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateClosedMessageforUnderCost
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReasonCodeForUnderCostMessage        = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReasonCodeForUnderCostMessage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverQuantityTolerance           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverQuantityCondition           = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconOverQuantityPercentage          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderQuantityTolerance          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderQuantityTolerance
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderQuantityCondition          = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderQuantityCondition
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReconUnderQuantityPercentage         = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderQuantityPercentage
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].CreateOpenMessageforUnderQuantity    = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateOpenMessageforUnderQuantity
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].CreateClosedMessageforUnderQuantity  = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateClosedMessageforUnderQuantity 
            MatchRuleToleranceArray.MatchRuleToleranceGroup[To].ReasonCodeForUnderQuantityMessage    = MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReasonCodeForUnderQuantityMessage
		    display "ToMatchUnderAmountToleranceIsNow:<MatchRuleToleranceArray.MatchRuleToleranceGroup[To].MatchUnderAmountTolerance>"

			invoke CopyTolerances MatchMessageToleranceRel
			    invoked.FromLevel = From
			    invoked.ToLevel = To

		ClearTolerances
		
		    initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].InvoiceMatchLevel 
		    initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchRuleType 
		    initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchToleranceOption 
		
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderAmountCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverAmountCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchOverQuantityPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].MatchUnderQuantityPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateChargeback
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverAmountPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderAmountPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateOpenMessageforUnderCost
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].CreateClosedMessageforUnderCost
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReasonCodeForUnderCostMessage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityCondition
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconOverQuantityPercentage
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderQuantityTolerance
            initialize MatchRuleToleranceArray.MatchRuleToleranceGroup[From].ReconUnderQuantityCondition
