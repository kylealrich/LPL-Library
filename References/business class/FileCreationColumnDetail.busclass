			initialize LocalJavaFormat

			include FileGeneration.FormatDate

			initialize LocalTimeFormat
			include FileGeneration.FormatTime
			LocalJavaFormat += LocalTimeFormat

			return LocalJavaFormat  

		CalculateAlphaFormat is a DerivedField
			type is Alpha 40
			restricted

			initialize LocalJavaFormat

			if (UpperCase)
				LocalJavaFormat = "%1S"
			else
				LocalJavaFormat = "%1s"

			if (EncloseInDoubleQuotes)
				LocalJavaFormat = "\"" + LocalJavaFormat + "\""
			return LocalJavaFormat 

		CalculateTextFormat is a DerivedField
