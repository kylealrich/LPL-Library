		NextRecordNumberPlus1 is a DerivedField
			type is Numeric 2
			restricted
			return NextRecordNumber + 1
			
		LargestEndPosition is a DerivedField
			type is Numeric 5
			restricted
			LocalNumeric2 = 0
			if (FileCreationSetup.FileType.Separated)
				return 0
			for each FixedLengthFieldRangeArray.FixedLengthFieldRange
				if (each.EndPosition = 0)
					return LocalNumeric2
				else
					if (each.EndPosition >= LocalNumeric2)
						LocalNumeric2 = each.EndPosition

		NextEndPositionAdditionalRecords is a DerivedField
			type is Numeric 5
			restricted

			return FileCreationRowDetail.NextBeginPositionAdditionalRecords + Length - 1		
	
		OverlapsExistInAdditionalRecordsMapping is a DerivedField
			type is Numeric 4
			restricted
			if (FixedLengthFieldRange.BeginPosition entered)

				LocalFixedLengthFieldRangeArray		= FixedLengthFieldRangeArray
				initialize LocalNumeric				
				
				for each (Each1) FixedLengthFieldRangeArray.FixedLengthFieldRange
					if (each (Each1).BeginPosition = 0)
						end for each
					increment LocalNumeric
					if (each (Each1) overlaps FixedLengthFieldRange)
						return ColumnOrder		
					LocalFixedLengthFieldRange = each (Each1)
					initialize LocalNumeric2				
					for each (Each2) LocalFixedLengthFieldRangeArray.FixedLengthFieldRange
						if (each (Each2).BeginPosition = 0)
							end for each
						increment LocalNumeric2
						if (LocalNumeric not = LocalNumeric2
						and each (Each2) overlaps LocalFixedLengthFieldRange)
							return ColumnOrder		
							

				for each (OtherColumns) FileCreationColumnDetailOtherRecordsRel
					LocalColumnOrder 			= each (OtherColumns).ColumnOrder
					LocalFixedLengthFieldRange2 = each (OtherColumns).FixedLengthFieldRange
					if (each (OtherColumns).FixedLengthFieldRange.BeginPosition entered)

						if (each (OtherColumns).FixedLengthFieldRange overlaps FixedLengthFieldRange)
							return LocalColumnOrder

						for each (CurrentRanges) LocalFixedLengthFieldRangeArray.FixedLengthFieldRange
							if (each (CurrentRanges).BeginPosition = 0)
								end for each
							if (each (CurrentRanges) overlaps LocalFixedLengthFieldRange2)
								return LocalColumnOrder

						if (each (OtherColumns).FixedLengthFieldRangeArray.FixedLengthFieldRange[1].BeginPosition entered)
							for each (OCFRA) each (OtherColumns).FixedLengthFieldRangeArray.FixedLengthFieldRange
								if (each (OCFRA).BeginPosition = 0)
									end for each
								LocalFixedLengthFieldRange = each(OCFRA)

								if (FixedLengthFieldRange overlaps LocalFixedLengthFieldRange)
									return LocalColumnOrder

								for each (CurrentColumn) LocalFixedLengthFieldRangeArray.FixedLengthFieldRange
									if (each (CurrentColumn).BeginPosition = 0)
										end for each
									if (each (CurrentColumn) overlaps LocalFixedLengthFieldRange)
										return LocalColumnOrder		

			return 0
						
