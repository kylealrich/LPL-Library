	 			if (FieldDataType.Decimal)
	        		include FileGeneration.TestReplace
	        			replace TestValue with TestValueDecimal
	  			if (FieldDataType.Numeric)
	        		include FileGeneration.TestReplace
	        			replace TestValue with TestValueNumeric
	   			if (FieldDataType.Date)
	        		include FileGeneration.TestReplace
	        			replace TestValue with TestValueDate
	   			if (FieldDataType.Time)
	        		include FileGeneration.TestReplace
	        			replace TestValue with TestValueTime
	   			if (FieldDataType.TimeStamp)
	        		include FileGeneration.TestReplace
	        			replace TestValue with TestValueTimeStamp
        	else
        		return ""		

