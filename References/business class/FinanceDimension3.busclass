		Move is an Instance Action
			valid when (SummaryOrPostingDimension)
			
			Parameters
				PrmContextDimension	is a FinanceDimension3ContextValue
				NewParent 			is a FinanceDimension3
							
			Parameter Rules
				PrmContextDimension
					initial value is FinanceDimension3
					
				NewParent
					default to DerivedTopNode
					required
					constraint (NewParent != FinanceDimension3)
						"CannotMoveToSelf"
					constraint (NewParent.DimensionType.Summary
					or			NewParent.DimensionType.Node)
						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension3Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<NewParent.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension3Label>"	
					TopNodeFound = false
					LocalFinanceDimension3 = NewParent
					if (!NewParent.DimensionType.Node)
						while (!TopNodeFound)
							constraint (FinanceDimension3 != LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension)
								"<FinanceDimension3>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension3 not entered)
								TopNodeFound = true
							else
								LocalFinanceDimension3 = LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension	
													
			Local Fields
				OldParent is like FinanceDimension3
																		
			Entrance Rules
				include CheckValidContext
							
				OldParent				 = DerivedParentDimension
				ParentDimension			 = NewParent

			Action Rules
				ParentDimension  	 = NewParent
				invoke Update DimensionInContextStructureRel
					invoked.ParentDimension		 	= NewParent

			Exit Rules
 				LocalFinanceDimension3 = FinanceDimension3
 							

				include CreateShadowForDimension		

				if (DimensionType.Summary)
					TopNodeFound = false
					while (!TopNodeFound)
						if (LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.DimensionType.Summary)
							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
							if (LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier entered)
								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension3 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension3 = LocalFinanceDimension3.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
					invoke NotifyMoveDimension3 NotifyBudgetTemplateRel
						invoked.PrmFinanceDimension3 = FinanceDimension3
						invoked.PrmNewParent		 = NewParent
						invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
