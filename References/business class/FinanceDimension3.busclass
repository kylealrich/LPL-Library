		RequestNewFinanceDimension3 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
				DimensionType
			    FinanceDimension3Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension3Label>Structure"
				DisplayDimension 				is like FinanceDimension3
					translatable
				PrmFinanceDimension3			is like FinanceDimension3
				ParentFinanceDimension3			is a FinanceDimension3
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
#ifdef module tx
				ProductTaxCategory		
#endif
#ifndef module tx
				ProductTaxCategory	is AlphaUpper size 15
#endif
				RevenuePartialAccount	is a FinanceCodeBlockNoProjectFD2
					default label is "PartialRevenueFinanceStructure"
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension3Structure
					initial value is first FinanceEnterpriseGroup.EnterpriseDim3Structure
					default to first FinanceEnterpriseGroup.EnterpriseDim3Structure
				DisplayDimension
					default to PrmFinanceDimension3
				PrmFinanceDimension3
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension3Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension3RegularExpression entered
					and DimensionType.Posting)
						constraint (PrmFinanceDimension3 matches FinanceEnterpriseGroup.FinanceDimension3RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension3Label>"
					LocalStructure 					= FinanceDimension3Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= DisplayDimension
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension3>"
				DimensionType
	    			default to CreateDimensionType		
					required
				BudgetIdentifier
					constraint (DimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (DimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalStructure					= FinanceDimension3Structure
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension3Label>"		
					constraint (!RequestSystemDimension3Rel exists)
						"<first RequestSystemDimension3Rel.DisplayDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension3Label>"
				Description
					required
				Active
					initial value is true		
#ifdef module tx
				ProductTaxCategory	
					constraint (CreateDimensionType.Posting)
						"ProductTaxCategoryOnlyValidForPostingDimensions"					
#endif
				RevenuePartialAccount
					constraint (CreateDimensionType.Posting)
						"PartialRevenueFinanceStructureOnlyValidForPostingDimensions"					
		
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension3			= PrmFinanceDimension3
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension3
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
#ifdef module tx
					invoked.ProductTaxCategory			= ProductTaxCategory
#endif
					invoked.RevenuePartialAccount		= RevenuePartialAccount
					if (ParentFinanceDimension3 entered)
						invoked.ParentDimension			= ParentFinanceDimension3
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
