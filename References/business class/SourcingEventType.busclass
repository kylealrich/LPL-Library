SourcingEventType is a BusinessClass
	owned by ss
	prefix is SET

	Ontology
		symbolic key is SourcingEventType

	Persistent Fields
		Description	is Alpha size 250
		Active		is Boolean

	Field Rules
		Description
			required
		Active
			default to true

	Attach Rules
		constraint (Active)
			"EventTypeIsInactive"

	Relations
		EventForTypeRel
			one-to-many relation to SourcingEvent
			Field Mapping uses ByType
				related.SourcingEventType = SourcingEventType
