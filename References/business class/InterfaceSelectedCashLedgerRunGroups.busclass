InterfaceSelectedCashLedgerRunGroups is a BusinessTask
    owned by cb

    Parameters
        CashManagementGroup
        InterfaceTransactions			is Boolean
        InterfacePayments				is Boolean
        InterfaceReceipts				is Boolean
        InterfaceReturnedPayments		is Boolean
        SelectRunGroupsLike				is AlphaUpper 15
        MoveErrorsToNewRunGroup			is Boolean
		ErrorRunGroupPrefix				is AlphaUpper 15
		CurrencyTable
		JournalizeReleasedDistributions	is Boolean
		AutoNumberingActionForTransactions	is Numeric 1
			States
				OverrideNumberWithAnAutoGeneratedNumber	value is 1
				BypassAutoTransactionNumbering			value is 2
		AutoNumberingActionForPayments	is Numeric 1
			States
				OverrideNumberWithAnAutoGeneratedNumber	value is 1
				BypassAutoTransactionNumbering			value is 2
		AutoNumberingActionForReceipts	is Numeric 1
			States
				OverrideNumberWithAnAutoGeneratedNumber	value is 1
				BypassAutoTransactionNumbering			value is 2
		AutoNumberingActionForEFT		is Numeric 1
			States
				OverrideNumberWithAnAutoGeneratedNumber	value is 1
				BypassAutoTransactionNumbering			value is 2		
		AutoNumberingActionForReturnedPayments	is Numeric 1
			States
				OverrideNumberWithAnAutoGeneratedNumber	value is 1
				BypassAutoTransactionNumbering			value is 2												
				
    Parameter Rules
        CashManagementGroup
        	required
        InterfaceTransactions
        	if (InterfacePayments			!entered
        	and InterfaceReceipts			!entered
        	and InterfaceReturnedPayments	!entered)
        		required
        			"SelectOneOrMoreRecordTypeToInterface"
		JournalizeReleasedDistributions													
			constraint (CashManagementGroup.GeneralLedgerSystemCodeRel.EncumbranceOption.NotUsed)
				"JournalizeDistributionsNotAllowedWhenBudgetingSetToTrackOrTrackAndEdit"       		
		AutoNumberingActionForTransactions
			if (InterfaceTransactions
			and CashManagementGroup.AutoTranNumberForTransactionForm)
				required
					"AutoNumberingActionSelectionRequired"
		AutoNumberingActionForPayments
			if (InterfacePayments
			and CashManagementGroup.AutoReferenceNumberForPaymentForm)
				required
					"AutoNumberingActionSelectionRequired"
		AutoNumberingActionForReceipts
			if (InterfaceReceipts
			and CashManagementGroup.AutoTranNumberForReceiptForm)
				required
					"AutoNumberingSelectionRequiredForReceipt"
		AutoNumberingActionForEFT
			if (InterfaceReceipts
			and CashManagementGroup.AutoReferenceNumberForEFTForm)
				required
					"AutoNumberingSelectionRequiredForEFT"
		AutoNumberingActionForReturnedPayments
			if (InterfaceReturnedPayments
			and CashManagementGroup.AutoTranNumberForReturnedPaymentForm)
				required
					"AutoNumberingActionSelectionRequired"
		SelectRunGroupsLike
			required
								
    Actions

        ScheduleInterface is an Instance Action
        	run in background

            Action Rules
				if (InterfaceTransactions)
					for each distinct CashLedgerTransactionImport.RunGroup in CashManagementGroup.CashLedgerTransactionImport set
						if (each.CashLedgerTransactionImport.RunGroup like SelectRunGroupsLike)
		                	invoke InterfaceCashLedgerTransactions CashLedgerTransactionImport in background
			            		invoked.PrmCashManagementGroup		   = CashManagementGroup
			            		invoked.PrmRunGroup					   = each.CashLedgerTransactionImport.RunGroup		
								invoked.PrmMoveErrorsToNewRunGroup	   = MoveErrorsToNewRunGroup
								invoked.PrmErrorRunGroupPrefix		   = ErrorRunGroupPrefix
								invoked.PrmCurrencyTable			   = CurrencyTable
								invoked.PrmJournalizeDistributions	   = JournalizeReleasedDistributions
								invoked.PrmAutoNumberingAction		   = AutoNumberingActionForTransactions
									
				if (InterfacePayments)
					for each distinct CashLedgerPaymentImport.RunGroup in CashManagementGroup.CashLedgerPaymentImport set
						if (each.CashLedgerPaymentImport.RunGroup like SelectRunGroupsLike)
		                	invoke InterfaceCashPayments CashLedgerPaymentImport in background
			            		invoked.PrmCashManagementGroup		   = CashManagementGroup
			            		invoked.PrmRunGroup					   = each.CashLedgerPaymentImport.RunGroup		
								invoked.PrmMoveErrorsToNewRunGroup	   = MoveErrorsToNewRunGroup
								invoked.PrmErrorRunGroupPrefix		   = ErrorRunGroupPrefix
								invoked.PrmCurrencyTable			   = CurrencyTable
								invoked.PrmJournalizeDistributions	   = JournalizeReleasedDistributions
								invoked.PrmAutoNumberingAction		   = AutoNumberingActionForPayments

				if (InterfaceReceipts)
					for each distinct CashLedgerReceiptImport.RunGroup in CashManagementGroup.CashLedgerReceiptImport set
						if (each.CashLedgerReceiptImport.RunGroup like SelectRunGroupsLike)
		                	invoke InterfaceCashReceipts CashLedgerReceiptImport in background
			            		invoked.PrmCashManagementGroup		   = CashManagementGroup
			            		invoked.PrmRunGroup					   = each.CashLedgerReceiptImport.RunGroup		
								invoked.PrmMoveErrorsToNewRunGroup	   = MoveErrorsToNewRunGroup
								invoked.PrmErrorRunGroupPrefix		   = ErrorRunGroupPrefix
								invoked.PrmCurrencyTable			   = CurrencyTable
								invoked.PrmJournalizeDistributions	   = JournalizeReleasedDistributions
								invoked.PrmAutoNumberingActionForRec   = AutoNumberingActionForReceipts
								invoked.PrmAutoNumberingActionForEFT   = AutoNumberingActionForEFT

				if (InterfaceReturnedPayments)
					for each distinct CashLedgerReturnedPaymentImport.RunGroup in CashManagementGroup.CashLedgerReturnedPaymentImport set
						if (each.CashLedgerReturnedPaymentImport.RunGroup like SelectRunGroupsLike)
		                	invoke InterfaceReturnedCashPayments CashLedgerReturnedPaymentImport in background
			            		invoked.PrmCashManagementGroup		   = CashManagementGroup
			            		invoked.PrmRunGroup					   = each.CashLedgerReturnedPaymentImport.RunGroup		
								invoked.PrmMoveErrorsToNewRunGroup	   = MoveErrorsToNewRunGroup
								invoked.PrmErrorRunGroupPrefix		   = ErrorRunGroupPrefix
								invoked.PrmCurrencyTable			   = CurrencyTable
								invoked.PrmJournalizeDistributions	   = JournalizeReleasedDistributions
								invoked.PrmAutoNumberingAction		   = AutoNumberingActionForReturnedPayments

