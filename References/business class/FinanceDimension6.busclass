		RequestNewFinanceDimension6 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
			    FinanceDimension6Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension6Label>Structure"
				DisplayDimension 				is like FinanceDimension6
					translatable
				PrmFinanceDimension6			is like FinanceDimension6
				ParentFinanceDimension6			is a FinanceDimension6
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension6Structure
					initial value is FinanceEnterpriseGroup.EnterpriseDim6Structure
					default to FinanceEnterpriseGroup.EnterpriseDim4Structure
				DisplayDimension
					default to PrmFinanceDimension6
				PrmFinanceDimension6
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension6Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension6RegularExpression entered
					and CreateDimensionType.Posting)
						constraint (PrmFinanceDimension6 matches FinanceEnterpriseGroup.FinanceDimension6RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension6Label>"
					LocalStructure 					= FinanceDimension6Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= PrmFinanceDimension6
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension6>"
				CreateDimensionType		
					required
				BudgetIdentifier
					constraint (CreateDimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (CreateDimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension6Label>"	
					constraint (FinanceDimension6Structure.DerivedSystemDimension = FinanceDimension6Structure.UndefinedMsg
						or FinanceDimension6Structure.DerivedSystemDimension = PrmFinanceDimension6)
						"<FinanceDimension6Structure.DerivedSystemDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension6Label>"
				Description
					required
				Active
					initial value is true
					if (SystemDimension)
						Active = true	
			
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension6			= PrmFinanceDimension6
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension6
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension6 entered)
						invoked.ParentDimension			= ParentFinanceDimension6
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
