			valid when (SummaryOrPostingDimension)
			
			Parameters
				PrmContextDimension is a FinanceDimension6ContextValue
				NewParent 			is a FinanceDimension6
							
			Parameter Rules
				PrmContextDimension
					initial value is FinanceDimension6
									
				NewParent
					default to DerivedTopNode
					required
					constraint (NewParent != FinanceDimension6)
						"CannotMoveToSelf"
					constraint (NewParent.DimensionType.Summary
					or			NewParent.DimensionType.Node)
						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension6Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<FinanceDimension6.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension6Label>"	
					TopNodeFound = false
					LocalFinanceDimension6 = NewParent
					if (!NewParent.DimensionType.Node)
						while (!TopNodeFound)
							constraint (FinanceDimension6 != LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension)
								"<NewParent>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension6 not entered)
								TopNodeFound = true
							else
								LocalFinanceDimension6 = LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension	
													
			Local Fields
				OldParent is like FinanceDimension6
																		
			Entrance Rules
				include CheckValidContext
							
				OldParent				 = DerivedParentDimension
				ParentDimension			 = NewParent

			Action Rules
				ParentDimension  	 = NewParent
				invoke Update DimensionInContextStructureRel
					invoked.ParentDimension		 	= NewParent

			Exit Rules
 				LocalFinanceDimension6 = FinanceDimension6
 							

				include CreateShadowForDimension		

				if (DimensionType.Summary)
					TopNodeFound = false
					while (!TopNodeFound)
						if (LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.DimensionType.Summary)
							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
							if (LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier entered)
								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension6 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension6 = LocalFinanceDimension6.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
					invoke NotifyMoveDimension6 NotifyBudgetTemplateRel
						invoked.PrmFinanceDimension6 = FinanceDimension6
						invoked.PrmNewParent		 = NewParent
						invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
