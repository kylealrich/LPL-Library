		InventoryPurge is a Set Action
			restricted
			Parameters
				PrmCompany								is an InventoryCompany
				PrmForecasts              				is AlphaUpper size 1
					default label is "Forecasts"
				    States
				        Yes value is "Y"
				        No  value is "N"
		       	PrmForecastYear         				is Numeric size 4
					default label is "Year"
				PrmForecastNumber          				is Numeric size 4
					default label is "Number"
				PrmPhysicalInventoryStatistics			is AlphaUpper size 1
					default label is "PhysicalInventoryStatistics"
				    States
				        Yes value is "Y"
				        No  value is "N"
				PrmPhysicalInventoryStatisticsYear		is Numeric size 4
					default label is "Year"
				PrmTransactionHistoryByPeriod			is AlphaUpper size 1
					default label is "TransactionHistory_byPeriod"
				    States
				        Yes value is "Y"
				        No  value is "N"
				PrmTransactionHistoryByPeriodYear		is Numeric size 4
					default label is "Year"
				PrmCostingRecords              			is AlphaUpper size 1
					default label is "CostingRecords"
				    States
				        Yes value is "Y"
				        No  value is "N"
				PrmCostingRecordsCutoffDate           		is Date
					default label is "CutoffDate"
				PrmHistoryTransactions                 	is AlphaUpper size 1
					default label is "HistoryTransactions"
				    States
				        Yes value is "Y"
				        No  value is "N"
				PrmHistoryTransactionsCutoffDate           is Date
					default label is "CutoffDate"
				PrmIssuedSerialNumbers                		is AlphaUpper size 1
					default label is "IssuedSerialNumbers"
				    States
				        Yes value is "Y"
				        No  value is "N"
				PrmIssuedSerialNumbersCutoffDate			is Date
					default label is "CutoffDate"
				PrmLotNumbers                   			is AlphaUpper size 1
					default label is "LotNumbers"
					States
					    Yes value is "Y"
					    No  value is "N"
				PrmPurgeOption								is Numeric 1
					default label is "ActionOption"
					States
						PurgeRecords 	value is 1
						ReportOnly		value is 2
			Parameter Rules
		        PrmCompany
		            required
					if(PrmHistoryTransactions = "Y" 
					or PrmCostingRecords = "Y" 
					or PrmIssuedSerialNumbers = "Y")
						constraint (PrmCompany.FinanceEnterpriseGroup.PurgeCutOffDate entered)
							"PurgeCutOffDateMustBeSetFor_Finance_Enterprise_Group<PrmCompany.FinanceEnterpriseGroup>"
		        PrmForecasts
		        	initial value is "N"
		            default to "N"		        	
		        PrmForecastYear
					if(PrmForecasts = "Y")
						required
							"ForecastYearIsRequired"
		            constraint (PrmForecastYear >= 1900 and PrmForecastYear <= 2100)
		                "<PrmForecastYear>ValueNotInRange"
		        PrmPhysicalInventoryStatistics
		        	initial value is "N"
		        PrmPhysicalInventoryStatisticsYear
		        	if(PrmPhysicalInventoryStatistics = "Y")
		        		required
							"PhysicalInventoryStatisticsYearIsRequired"
		            constraint (PrmPhysicalInventoryStatisticsYear >= 1900 and PrmPhysicalInventoryStatisticsYear <= 2100)
		                "<PrmPhysicalInventoryStatisticsYear>ValueNotInRange"					
		        PrmTransactionHistoryByPeriod
		        	initial value is "N"
		        PrmTransactionHistoryByPeriodYear
		        	if(PrmTransactionHistoryByPeriod = "Y")
		        		required
							"TransactionHistoryByPeriodYearIsRequired"
		            constraint (PrmTransactionHistoryByPeriodYear >= 1900 and PrmTransactionHistoryByPeriodYear <= 2100)
		                "<PrmTransactionHistoryByPeriodYear>ValueNotInRange"
		        PrmCostingRecords
		        	initial value is "N"
		        PrmHistoryTransactions
		        	initial value is "N"
					
		        PrmIssuedSerialNumbers
		        	initial value is "N"					
		        PrmLotNumbers
		        	initial value is "N"
		    Instance Selection
		    	where(Company			= PrmCompany)
		    Local Fields
		    	LocalActor											is an Actor
		    	LocalNotification									is LPLText
		    	LocalNewLine										is LPLText
    			LocalTab											is LPLText
    		
		    	LocalForecast										is Numeric 10
		    	LocalPhysicalInventoryAccuracy						is Numeric 10
		    	LocalInventoryUsageHistory							is Numeric 10
		    	LocalInventoryCostHistory							is Numeric 10
		    	LocalItemSerialNumber								is Numeric 10
		    	LocalItemLot										is Numeric 10
		    	LocalInventoryTransactionPurge						is Numeric 10
		    	LocalInventoryTransactionLinePurge					is Numeric 10
				
		    Rule Blocks
		    	UserNotif
		    		LocalNewLine 	= "\u000a"  
		    		LocalTab		= "\u0009"
		    		if(PrmPurgeOption.PurgeRecords)
						LocalNotification += LocalNewLine+"Number of records that have been purged"+LocalNewLine
					else
						LocalNotification += LocalNewLine+"Number of records that are available for purge"+LocalNewLine
					LocalNotification += "InventoryForecast: "+LocalForecast +LocalNewLine +"PhysicalInventoryAccuracy: "+LocalPhysicalInventoryAccuracy+LocalNewLine
					LocalNotification += "InventoryUsageHistory: " +LocalInventoryUsageHistory +LocalNewLine +"InventoryCostHistory: "+LocalInventoryCostHistory +LocalNewLine
					LocalNotification += "InventoryTransaction: " +LocalInventoryTransactionPurge +LocalNewLine// + "InventoryTransactionLine: "+ LocalInventoryTransactionLinePurge+LocalNewLine
					LocalNotification += "ItemSerialNumber: "+LocalItemSerialNumber +LocalNewLine +"ItemLot: "+LocalItemLot
											
			Action Rules
				Set Rules
					Entrance Rules
						LocalActor = actor
						LocalForecastNumber							= 	PrmForecastNumber
						LocalForecastYear							= 	PrmForecastYear
						LocalPhysicalInventoryStatisticsYear 		=	PrmPhysicalInventoryStatisticsYear
						LocalTransactionHistoryByPeriodYear			=	PrmTransactionHistoryByPeriodYear
						LocalPurgeCutOffDate						=	PrmCompany.FinanceEnterpriseGroup.PurgeCutOffDate
					Exit Rules
						include UserNotif
						send notification
							to LocalActor
							description is "InventoryPurgeDetailsFor_Company<PrmCompany>"
							priority is high
							detail is "Date&Time:<current timestamp><LocalNewLine>\Username:<actor><LocalNewLine>\Company:<PrmCompany>\<LocalNotification>"
				Instance Rules
					if(PrmForecasts.Yes)
						LocalForecast 									= instance count of InventoryForeCastRel
						invoke InventoryForecastPurge InventoryForecast
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmForecastYear 					= PrmForecastYear
							invoked.PrmForecastNumber 					= PrmForecastNumber
							invoked.PrmPurgeOption						= PrmPurgeOption
						
					if(PrmPhysicalInventoryStatistics.Yes)
						LocalPhysicalInventoryAccuracy 					= instance count of PhysicalInventoryAccuracyRel
						invoke PhysicalInventoryAccuracyPurge PhysicalInventoryAccuracy
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmPhysicalInventoryStatisticsYear	= PrmPhysicalInventoryStatisticsYear
							invoked.PrmPurgeOption						= PrmPurgeOption
						
					if(PrmTransactionHistoryByPeriod.Yes)
						LocalInventoryUsageHistory						= instance count of InventoryUsageHistoryRel
						invoke InventoryUsageHistoryPurge InventoryUsageHistory
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmTransactionHistoryByPeriodYear	= PrmTransactionHistoryByPeriodYear
							invoked.PrmPurgeOption						= PrmPurgeOption
						
						
					if(PrmCostingRecords.Yes)
						LocalInventoryCostHistory						= instance count of InventoryCostHistoryRel
						invoke InventoryCostHistoryPurge InventoryCostHistory
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmCostingRecordsCutoffDate			= LocalPurgeCutOffDate
							invoked.PrmPurgeOption						= PrmPurgeOption
						
					if(PrmHistoryTransactions.Yes)
						LocalInventoryTransactionPurge					= instance count of InventoryTransactionPurgeRel
						invoke InventoryTransactionPurge InventoryTransaction
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmHistoryTransactionsCutoffDate	= LocalPurgeCutOffDate
							invoked.PrmPurgeOption						= PrmPurgeOption
						
					if(PrmIssuedSerialNumbers.Yes)
						LocalItemSerialNumber							= instance count of ItemSerialNumberRel
						invoke ItemSerialNumberPurge ItemSerialNumber
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmIssuedSerialNumbersCutoffDate	= LocalPurgeCutOffDate
							invoked.PrmPurgeOption						= PrmPurgeOption
						
					if(PrmLotNumbers.Yes)
						LocalItemLot 									= instance count of ItemLotRel
						invoke ItemLotPurge ItemLot
							invoked.PrmCompany 							= PrmCompany
							invoked.PrmPurgeOption						= PrmPurgeOption
						
		CreateLocationsForAContract is a Set Action 
		
		CreateLocationsForAContractInstance is an Instance Action
			restricted 
			Parameters
				ParmContractGroup   		is a ProcurementGroup
				ParmContract            	is a Contract
				CompanyOption           	is Numeric 1
				LineLevel               	is Boolean 
				ParmRequestingLocation  	is a RequestingLocation 
				ParmInventoryLocation   	is an InventoryLocation

			Action Rules 
				LocalContractGroup = ParmContractGroup 
				LocalContract      = ParmContract 
				if ((CompanyOption = 0
				or   CompanyOption = 1)
				and  ParmRequestingLocation !entered)
					if (ParmInventoryLocation !entered)
						invoke CreateContractLocationsSet InventoryLocation 
							invoked.ParmContractGroup      = ParmContractGroup 
							invoked.ParmContract           = ParmContract 
							invoked.ParmCompany			   = Company 
							invoked.LineLevel              = LineLevel
					else 
					if  (ParmInventoryLocation entered
					and (ContractGroupRel.DoNotUseParLocations = false
					or  (ContractGroupRel.DoNotUseParLocations
					and  ParmInventoryLocation.IsPerpetualLocation)))
						LocalInventoryLocation = ParmInventoryLocation
						if (ContractLocationRel !exists)
							invoke Create ContractAndLineLocation 
								invoked.ContractGroup 						= ParmContractGroup 
								invoked.Contract                			= ParmContract 
								invoked.ContractAndLineLocation.Company 	= Company
								invoked.ContractAndLineLocation.Location	= ParmInventoryLocation 
								invoked.ClinicalSystemUse                   = true	
								invoked.LineLevel                           = LineLevel 
						else 
						if (ContractLocationRel exists
						and ContractLocationRel.ClinicalSystemUse = false)
							invoke Update ContractLocationRel
								invoked.ClinicalSystemUse = true
								invoked.LineLevel         = LineLevel 
				if ((CompanyOption = 0
				or   CompanyOption = 2)
				and  ContractGroupRel.DoNotUseRequestingLocations = false)
					if (ParmRequestingLocation !entered)
						invoke CreateContractLocations RequestingLocation 
							invoked.ParmContractGroup      = ParmContractGroup 
							invoked.ParmContract           = ParmContract 
							invoked.ParmCompany			   = Company 
							invoked.LineLevel              = LineLevel					
					else 
					if (ParmRequestingLocation entered)
						if  (ParmRequestingLocation.InventoryTransactionType.Issue
						and  (ContractGroupRel.DoNotUseParLocations = false
						or   (ContractGroupRel.DoNotUseParLocations
						and  !ParmRequestingLocation.ParOrExchangeLocation)))
							LocalRequestingLocation = ParmRequestingLocation
							if (ContractRequestingLocationRel !exists
							and ContractRequestingInventoryLocationRel !exists)
								invoke Create ContractAndLineLocation 
									invoked.ContractGroup 								= ParmContractGroup 
									invoked.Contract                					= ParmContract 
									invoked.ContractAndLineLocation.Company 			= Company
									invoked.ContractAndLineLocation.RequestingLocation	= ParmRequestingLocation 
									invoked.ClinicalSystemUse                   		= true	
									invoked.LineLevel                                   = LineLevel 
							else
							if (ContractRequestingLocationRel exists
							and ContractRequestingLocationRel.ClinicalSystemUse = false)
								invoke Update ContractRequestingLocationRel
									invoked.ClinicalSystemUse = true
									invoked.LineLevel         = LineLevel 					

		DeleteLocationsForAContract is a Set Action 
			restricted 
			Parameters
				ParmContractGroup   	is a ProcurementGroup
				ParmContract            is a Contract
				ParmCompany             is like InventoryCompany
			Instance Selection
				where (ParmCompany              = Company)

			Action Rules 
				Instance Rules 
					LocalContractGroup = ParmContractGroup 
					LocalContract      = ParmContract 
					for each CompanyActiveLocationRel
						LocalInventoryLocation = each.InventoryLocation
						if (ContractLocationRel exists)
							invoke DeleteClinical ContractLocationRel 
					for each CompanyActiveRequestingLocationRel
						LocalRequestingLocation = each.RequestingLocation
						if (ContractRequestingLocationRel exists)
							invoke DeleteClinical ContractRequestingLocationRel 

		InventoryLocationAuditPurge is a Set Action
			restricted
			Parameters
				PrmCompany								is an InventoryCompany
					default label is "Company"
				PrmPurgeOption							is Numeric 1
					default label is "ActionOption"
					States
						ReportOnly		value is 0
						PurgeRecords 	value is 1
				PrmAllLocations							is Boolean
					default label is "AllLocations"
				PrmInventoryLocation					is an InventoryLocation
					default label is "InventoryLocation"
			Parameter Rules
				PrmCompany
					required
				PrmPurgeOption
					initial value is PrmPurgeOption.ReportOnly
					default to PrmPurgeOption.ReportOnly
				PrmAllLocations
					initial value is true
				PrmInventoryLocation
					if (PrmAllLocations)
						cannot be entered
							"CannotBeEnteredWhenProcessingAllLocations"
					else
						required
			Instance Selection
				include deleted records
				where (Company =  PrmCompany)

		    Local Fields
		    	LocalActor											is an Actor
		    	LocalNotification									is LPLText
		    	LocalNewLine										is LPLText
    			LocalTab											is LPLText
    			LocalLocation										is Alpha 20
		    	LocalItemLocationAuditCount							is Numeric 10
		    	LocalStockOnHandDetailAuditCount					is Numeric 10

			Action Rules
				Empty Set Rules
					LocalActor = actor
					send notification
						to LocalActor
						description is "NoInventoryLocationAuditRecordsFoundToPurge"
						priority is high
						detail is "ThereAreNoInventoryLocationAuditRecordsPurged"
				Set Rules
					Exit Rules
						LocalActor = actor
			    		LocalNewLine 	= "\u000a"  
			    		LocalTab		= "\u0009"
			    		if(PrmPurgeOption.PurgeRecords)
							LocalNotification += LocalNewLine + "Number of records that have been purged" + LocalNewLine
						else
							LocalNotification += LocalNewLine + "Number of records that are available for purge" + LocalNewLine
												
						LocalNotification += "ItemLocationAudit: " + LocalItemLocationAuditCount + LocalNewLine + "StockOnHandDetailAudit: "+ LocalStockOnHandDetailAuditCount
						
						if (PrmAllLocations)
							LocalLocation = "All Locations"
						else
							LocalLocation = PrmInventoryLocation
						
						send notification
							to LocalActor
							description is "InventoryLocationAuditPurgeDetailsFor_Company<PrmCompany>"
							priority is high
							detail is "Company:<PrmCompany>\Location:<LocalLocation>\<LocalNotification>"
				
				Instance Rules
					LocalAllLocations 		= PrmAllLocations
					LocalInventoryLocation	= PrmInventoryLocation
					if (InventoryLocationPurgeRel exists)
						for each InventoryLocationPurgeRel
							LocalItemLocationAuditCount			+= each.DerivedItemLocationAuditCount
							LocalStockOnHandDetailAuditCount	+= each.DerivedStockOnHandDetailAuditCount
						if (PrmPurgeOption.PurgeRecords)
							invoke AuditPurge InventoryLocation in foreground
								invoked.PrmCompany 				= PrmCompany
								invoked.PrmAllLocations			= PrmAllLocations
								invoked.PrmInventoryLocation	= PrmInventoryLocation	

