    Field Rules
        Name
            required

        RequireUnitOfMeasure
            initial value is true

        AllocationBasis
            default to "D"

        InternalOnly
            default to "N"

        WorkOrderInUse
            default to false

		ItemGroup
			initial value is Company.BusinessGroup
            required			
			constraint (ItemGroup = Company.BusinessGroup)
				"ItemGroup<ItemGroup>MustBeSameAsVendorBusinessGroup<Company.BusinessGroup>OnGlobalLedgerCompany"
			if (PurchasingCompanyRel exists)
				constraint (ItemGroup = PurchasingCompanyRel.ProcurementGroup)
					"ProcurementGroupAssociatedWithPurchasingCompany<PurchasingCompanyRel.ProcurementGroup>CannotBeDifferentFromProcurementGroupAssociatedWithItemGroup<ItemGroup>"


			if (ItemGroup changed)
				constraint (!ItemLocationExists)
					"CannotChangeItemGroup;ItemLocationExists"                


		CostingMethod
            required
			if (CostingMethod.StandardCosting)
				constraint (!NoChargeItemLocationExists)
					"CostMethodCannotBeStd;NoChargeItemsExist"                


			if (CostingMethod.FirstInFirstOut
			or  CostingMethod.LastInFirstOut)
				constraint (!AllowNegativeStockOnHand)
					"Negative_\S\O\HAllowedOnlyForStd/avgCostCpy"             


		CurrentYear
			if (CurrentYear changed)
				constraint (!InventoryTransactionExists)
					"CannotChangeCurrentYear;InventoryTransactionsExist"


		LastYearEndDate
            required
			constraint (LastYearEndDate < YearPeriodStatusArray.YearPeriodStatus[1].YearPeriod)
				"LastYearEndDateMustBePriorToFirstPeriodEndDate"


		AllocationMethod
            default to "O"
			if (AllocationMethod changed)
				constraint (!UnallocatedQuantityExists)
					"CannotChgMethod,UnallocatedQtysExistAtLoc"               










				if (old WorkOrderInUse 
				and !WorkOrderInUse) 
					constraint (!WorkOrderLocationExists)
						"CannotChange;InventoryLocation<first WorkOrderLocationRel.InventoryLocation>UsingWorkOrder"







		YearPeriodStatusArray
			initialize TempCounter1
			initialize TempCounter2
			while (TempCounter1 < 53)
				TempCounter1 += 1
				if (TempCounter1 > 1)
					TempCounter2 = TempCounter1 - 1

				if ((NumberOfPeriods.12PeriodsPerYearMonthly
				and  TempCounter1 > 12)
				or  (NumberOfPeriods.13PeriodsPerYear
				and  TempCounter1 > 13)
				or  (NumberOfPeriods.52PeriodsPerYearWeekly
				and  TempCounter1 > 52))
					constraint (YearPeriodStatusArray.YearPeriodStatus[TempCounter1].YearPeriod not entered)
						"OnlyEnterDatesForTheNumberOfPeriods"                    

				else

					constraint (YearPeriodStatusArray.YearPeriodStatus[TempCounter1].YearPeriod entered)

						"MustEnterPeriodEndDateForAllPeriods;Period<TempCounter1>"

				if (YearPeriodStatusArray.YearPeriodStatus[TempCounter1].YearPeriod entered)
					constraint (YearPeriodStatusArray.YearPeriodStatus[TempCounter1].YearPeriod > YearPeriodStatusArray.YearPeriodStatus[TempCounter2].YearPeriod)
						"EnterDatesInIncreasingOrder"                            


		BinTypeArray
			initialize TempCounter1
			while (TempCounter1 < 9)
				TempCounter1 += 1
				if (BinTypeArray.BinTypeGroup[TempCounter1].BinType entered)
					TempCounter2 = 1





				else
					if (old BinTypeArray.BinTypeGroup[TempCounter1].BinType entered)
						LocalBinType = old BinTypeArray.BinTypeGroup[TempCounter1].BinType
						constraint (BinTypeRel not exists)
							"BinTypeIsInUse;CannotDelete"


