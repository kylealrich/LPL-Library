		CloseInventoryControlSystem is a Set Action
			run in foreground
			restricted
			Parameters
				FinanceEnterpriseGroup
				PrmCompany					is an InventoryCompany
					default label is "Company"
				PrmCompanyGroup				is a GeneralLedgerCompanyGroup		
					default label is "CompanyGroup"
			
			Local Fields
				LocalCompany											is an InventoryCompany
				LocalActor												is an Actor
				LocalInventoryTransaction 								is like InventoryTransaction
				LocalNotificationDetails								is LPLText
				LocalCompaniesCurrentPeriodClosed						is LPLText
				LocalCompaniesCounter									is Numeric 4
				LocalCompaniesCurrentPeriodClosedCounter				is Numeric 4
				
			Parameter Rules
				FinanceEnterpriseGroup
					initial value is actor.context.FinanceEnterpriseGroup
				PrmCompany
					if (PrmCompany entered)
						constraint (PrmCompanyGroup not entered)
							"CannotEnterBoth_CompanyAnd_Company_Group"
					if (PrmCompany not entered)
						constraint (PrmCompanyGroup entered)
							"MustEnterEither_CompanyOr_Company_Group"
					constraint (PrmCompany.ICSystemClosingControlRel.Control = true)
						"CompanyIsNotSetUpFor_Close_Control"
					display "EntityCurrentPeriod:<PrmCompany.ICSystemClosingControlRel.EntityCurrentPeriod>"
					display "ClosedPeriodGeneralLedgerCalendarPeriod:<PrmCompany.ICSystemClosingControlRel.ClosedPeriod.GeneralLedgerCalendarPeriod>"
					
					LocalInventoryTransaction = first PrmCompany.UnreleasedInventoryTransactionsRel.InventoryTransaction
					
					constraint (PrmCompany.ICSystemClosingControlRel.CurrentPeriodNotClosed)
						"PeriodIsAlreadyClosedFor_Inventory_Control.ClosePeriodFor_Global_LedgerToRunAgain."
						
					constraint (PrmCompany.POSystemClosePrerequisitesComplete)
						"ThePurchasingSystemMustBeClosedFor_Company<PrmCompany>PriorToClosingTheInventorySystem"
					
					constraint (PrmCompany.BLSystemClosePrerequisitesComplete)
						"TheBillingSystemMustBeClosedFor_Company<PrmCompany>PriorToClosingTheInventorySystem"
					
					constraint (PrmCompany.UnreleasedInventoryTransactionsRel not exist)
						"CannotClose,UnreleasedInventoryTransactionsExistFor_Company<PrmCompany>_-_<LocalInventoryTransaction>"
					
					constraint (PrmCompany.ReleasedUnpostedTransactionsRel not exist)	
						"CannotClose;Released,UnpostedInventoryTransactionsExistFor_Company<PrmCompany>.Run_Journalize_DistributionsAction"	
						
				PrmCompanyGroup
					if (PrmCompanyGroup entered)
						LocalCompanyGroup = PrmCompanyGroup
						constraint (PrmCompanyGroup.ICSystemClosingControlCompaniesWithControlRel exists)
							"NoCompanyInThe_Company_GroupIsSetUpFor_Inventory_System_Close_Control"
						constraint (PrmCompanyGroup.ICSystemClosingControlCompaniesWithOpenPeriodRel exists)
							"PeriodIsAlreadyClosedFor_Inventory_ControlForAllCompaniesWith_Inventory_System_Closing_ControlInThe_Company_Group"
						
						for each PrmCompanyGroup.GeneralLedgerCompanyGroupMember set
							LocalCompany = each.GeneralLedgerCompanyGroupMember.Company
							
							if (LocalCompany.ICSystemClosingControlRel.Control
							and LocalCompany.ICSystemClosingControlRel.CurrentPeriodNotClosed)
								
								LocalInventoryTransaction = first LocalCompany.UnreleasedInventoryTransactionsRel.InventoryTransaction
								
								constraint (LocalCompany.POSystemClosePrerequisitesComplete)
									"ThePurchasingSystemMustBeClosedFor_Company<each.Company>PriorToClosingTheInventorySystem"
								constraint (LocalCompany.BLSystemClosePrerequisitesComplete)
									"TheBillingSystemMustBeClosedFor_Company<each.Company>PriorToClosingTheInventorySystem"
								constraint (LocalCompany.UnreleasedInventoryTransactionsRel not exist)
									"CannotClose,UnreleasedInventoryTransactionsExistFor_Company<each.Company>_-_<LocalInventoryTransaction>"
								constraint (LocalCompany.ReleasedUnpostedTransactionsRel not exist)	
									"CannotClose;Released,UnpostedInventoryTransactionsExistFor_Company<each.Company>.Run_Journalize_DistributionsAction"
						
			Instance Selection
				where (((PrmCompany entered 
				and      Company							= PrmCompany)
        		or		(PrmCompanyGroup entered 
        		and      CompanyGroupRel exists))	
				and      ICSystemClosingControlRel.Control 	= true)
				
			Sort Order
				Company

			Action Rules
				Set Rules
					Entrance Rules
						LocalActor = actor
					Exit Rules
						if (PrmCompanyGroup entered)
							if (LocalCompaniesCurrentPeriodClosedCounter entered)
								LocalNotificationDetails += "Inventory system closing completed for company group " + PrmCompanyGroup + "."
								LocalNotificationDetails += "\u000a"
								LocalNotificationDetails += "Unable to process " + LocalCompaniesCurrentPeriodClosedCounter + " out of " + LocalCompaniesCounter + " companies. "
								LocalNotificationDetails += "Period is already closed for inventory control for company: " + LocalCompaniesCurrentPeriodClosed + " with inventory system closing control."
								send notification
									to LocalActor
									description is "InventorySystemClosingCompletedForCompanyGroup<PrmCompanyGroup>"
									priority is high
									detail is "<LocalNotificationDetails>"
							else
								send notification
									to LocalActor
									description is "InventorySystemClosingCompletedForCompanyGroup<PrmCompanyGroup>"
									priority is high
									detail is "InventorySystemClosingCompletedForCompanyGroup<PrmCompanyGroup>"

				Instance Rules

					increment LocalCompaniesCounter
				
					if (Company.ICSystemClosingControlRel.CurrentPeriodNotClosed)
						invoke UpdateCompanySystemClosingControl ICSystemClosingControlRel
							invoked.PrmCompany		 			= Company
							invoked.PrmSystem				 	= "IC"
					else
						increment LocalCompaniesCurrentPeriodClosedCounter
						if (LocalCompaniesCurrentPeriodClosedCounter = 1)
							LocalCompaniesCurrentPeriodClosed 	= Company
						else
							LocalCompaniesCurrentPeriodClosed	+= ", "
							LocalCompaniesCurrentPeriodClosed 	+= Company

					if (PrmCompanyGroup not entered)
						send notification
							to LocalActor
							description is "InventorySystemClosingCompleted.Company:<Company>_<Company.Name>.Period:<Company.ICSystemClosingControlRel.ClosedPeriod.PeriodName>"
							priority is high
							detail is "InventorySystemClosingCompleted.Company:<Company>_<Company.Name>.Period:<Company.ICSystemClosingControlRel.ClosedPeriod.PeriodName>"
			
