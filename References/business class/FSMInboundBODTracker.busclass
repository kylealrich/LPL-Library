		PurgeBODRecords is a Set Action
			default label is "PurgeBO\DRecords"
			restricted
			run in background
			confirmation required
				"Warning:ThisWillPermanentlyDeleteTheSelectedRecords.DoYouWantToContinue?"
			no records message is "PurgeBODRecordsSuccessfullyInitiated"
			Parameters
				PurgeAllRecords			is Boolean
				PurgeDate				is Date
				PurgeRecords			is a UnitsAndUnitType
			Parameter Rules
				PurgeAllRecords
					constraint (PurgeDate not entered
					and 		PurgeRecords not entered
					and 		PurgeAllRecords entered)
						"CannotProceed;_PleaseClearOneOfTheOptionsCurrentlyInUse"

					if (PurgeDate not entered
					and	PurgeRecords not entered
					and not PurgeAllRecords)
						required
							"CannotProceed;_PleaseSelectAPurgeOption"

				PurgeDate
					constraint (PurgeAllRecords	not entered
					and 		PurgeRecords not entered
					and 		PurgeDate entered)
					 	"CannotProceed;_PleaseClearOneOfTheOptionsCurrentlyInUse"
				
				PurgeRecords
					constraint (PurgeRecords.Units > 0)
						"UnitsCannotBeNegative"
						
					if (PurgeRecords.UnitType.Days)
						PurgeDate = system current corporate date - PurgeRecords.Units as days
					else
					if (PurgeRecords.UnitType.Months)
						PurgeDate = system current corporate date - PurgeRecords.Units as months
					else	
					if (PurgeRecords.UnitType.Years)
						PurgeDate = system current corporate date - PurgeRecords.Units as years
			
			Instance Selection
				where (Status.Completed  and ((StartDate < PurgeDate) or (PurgeAllRecords)))
			Action Rules
				Instance Rules
					invoke Purge
		
