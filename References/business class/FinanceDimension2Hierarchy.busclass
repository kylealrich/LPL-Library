				LocalFinanceDimension2 = FinanceDimension2
			else
				while (LocalFinanceDimension2.Level > LocalLevel)
					LocalFinanceDimension2			= LocalDimensionHierarchyRel.FinanceDimension2Hierarchy.ParentDimension

	Derived Fields
