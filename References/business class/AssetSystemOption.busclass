            classic name is OLD-SPLIT-FLAG
        NewDistribution                  is AlphaUpper size 1
            classic name is NEW-DISTRIB-FL
            States
                Detail  value is "D"
                Summary value is "S"
        OldDistribution                  is AlphaUpper size 1
            classic name is OLD-DISTRIB-FL
            States
                Detail  value is "D"
                Summary value is "S"
        NewEventAccount                  is Boolean
            classic name is NEW-EVENT-ACCT
        OldEventAccount                  is Boolean
            classic name is OLD-EVENT-ACCT

