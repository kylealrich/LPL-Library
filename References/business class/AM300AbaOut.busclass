        AbaLastClose            is like InternationalAmount
        AbaFirstLastFl          is like FirstAndLastYear
        AbaInactYtd             is like YearToDateInactiveMonths
        AbaInactLtd             is like LifeToDateInactiveMonths
        AbaSalvageValue         is like InternationalAmount
        AbaToSalvValue          is like InternationalAmount
        AbaBusPercent           is like BusinessPercent
        AbaToBusPercent         is like BusinessPercent
        AbaPersonalUse          is like PersonalUseCode
        AbaToPersonlUse         is like PersonalUseCode
        AbaYr1Exp               is like InternationalAmount
        AbaToYr1Exp             is like InternationalAmount
        AbaTcTable              is like AssetTaxCreditTable
