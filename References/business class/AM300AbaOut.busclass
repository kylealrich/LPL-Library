        AbaDeprYtdAllow         is like InternationalAmount
        AbaDeprYtdOvrrd         is like InternationalAmount
        AbaToDpYtdOvrd          is like InternationalAmount
        AbaDeprLtd              is like InternationalAmount
        AbaToDeprLtd            is like InternationalAmount
