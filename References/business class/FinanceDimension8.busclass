		RequestNewFinanceDimension8 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
			    FinanceDimension8Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension8Label>Structure"
				DisplayDimension 				is like FinanceDimension8
					translatable
				PrmFinanceDimension8			is like FinanceDimension8
				ParentFinanceDimension8			is a FinanceDimension8
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension8Structure
					initial value is FinanceEnterpriseGroup.EnterpriseDim8Structure
					default to FinanceEnterpriseGroup.EnterpriseDim8Structure
				DisplayDimension
					default to PrmFinanceDimension8
				PrmFinanceDimension8
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension8Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension8RegularExpression entered
					and CreateDimensionType.Posting)
						constraint (PrmFinanceDimension8 matches FinanceEnterpriseGroup.FinanceDimension8RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension8Label>"
					LocalStructure 					= FinanceDimension8Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= PrmFinanceDimension8
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension8>"
				CreateDimensionType		
					required
				BudgetIdentifier
					constraint (CreateDimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (CreateDimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension8Label>"	
					constraint (FinanceDimension8Structure.DerivedSystemDimension = FinanceDimension8Structure.UndefinedMsg
						or FinanceDimension8Structure.DerivedSystemDimension = PrmFinanceDimension8)
						"<FinanceDimension8Structure.DerivedSystemDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension8Label>"
				Description
					required
				Active
					initial value is true
					if (SystemDimension)
						Active = true
			
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension8			= PrmFinanceDimension8
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension8
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension8 entered)
						invoked.ParentDimension			= ParentFinanceDimension8
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
