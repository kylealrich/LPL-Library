	Actions 

		Create is a Create Action 
			restricted
			Action Rules 
				CreateDate    = current corporate date
			
			Exit Rules 
				invoke Create ConsolidatedItemMasterUOM
					invoked.ContractGroup			= ContractGroup 
					invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
				LocalContract		= TransientContract 
				LocalContractLine   = TransientContractLine 
				
				LocalManufacturer   	= Manufacturer 
				LocalManufacturerNumber	= ManufacturerNumber
				
				if (TransientCreateLocation = true           
				or (TransientContractEntered
				and InternalItem exists))
					for each ItemLocationRel  
						LocalCompany 			= each.Company 
						LocalLocation           = each.InventoryLocation 
						if (ConsolidatedItemLocationExistsRel !exists
						and InventoryForReqLocRel !exists)
							invoke Create ConsolidatedItemLocation 
								invoked.ContractGroup			= ContractGroup 
								invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
								invoked.InventoryCompany        = each.Company 
								invoked.InventoryLocation       = each.InventoryLocation 
								invoked.ContractOrItemLocation  = 1  				
				if (TransientContract entered)
					if (ContractLineRel.ClinicalSystemUse)
						ClinicalSystemUse = true 
						for each ContractAndLineLocationRel 
							LocalCompany 					= each.ContractAndLineLocation.Company 
							LocalLocation          			= each.ContractAndLineLocation.Location 
							LocalRequestingLocation         = each.ContractAndLineLocation.RequestingLocation 
							if (ConsolidatedItemLocationExistsRel !exists)
								if (each.ContractAndLineLocation.Location entered
								and InventoryForReqLocRel !exists)
									invoke Create ConsolidatedItemLocation 
										invoked.ContractGroup			= ContractGroup 
										invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
										invoked.InventoryCompany        = each.ContractAndLineLocation.Company 
										invoked.InventoryLocation       = each.ContractAndLineLocation.Location 
										invoked.RequestingLocation      = each.ContractAndLineLocation.RequestingLocation 
										invoked.ContractOrItemLocation  = 2
								else 
								if (each.ContractAndLineLocation.RequestingLocation entered
								and RequestingLocForInventoryRel !exists)
									invoke Create ConsolidatedItemLocation 
										invoked.ContractGroup			= ContractGroup 
										invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
										invoked.InventoryCompany        = each.ContractAndLineLocation.Company 
										invoked.InventoryLocation       = each.ContractAndLineLocation.Location 
										invoked.RequestingLocation      = each.ContractAndLineLocation.RequestingLocation 
										invoked.ContractOrItemLocation  = 2							
				else 
				if (TransientCompany entered)
					LocalCompany 			= TransientCompany 
					LocalLocation           = TransientLocation 
					if (ConsolidatedItemLocationExistsRel !exists
					and InventoryForReqLocRel !exists)
						invoke Create ConsolidatedItemLocation 
							invoked.ContractGroup			= ContractGroup 
							invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
							invoked.InventoryCompany        = TransientCompany 
							invoked.InventoryLocation       = TransientLocation   
							invoked.ContractOrItemLocation  = 1   

				if (InternalItem exists
				and TransientContract !entered)
				 	for each ContractLinesByItemForLocationRel
						LocalContract     = each.Contract 
						LocalContractLine = each.ContractLine
						for each ContractAndLineLocationRel 
							LocalCompany 					= each.ContractAndLineLocation.Company 
							LocalLocation          			= each.ContractAndLineLocation.Location 
							LocalRequestingLocation         = each.ContractAndLineLocation.RequestingLocation 
							if (ConsolidatedItemLocationExistsRel !exists)
								if (each.ContractAndLineLocation.Location entered
								and InventoryForReqLocRel !exists)
									invoke Create ConsolidatedItemLocation 
										invoked.ContractGroup			= ContractGroup 
										invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
										invoked.InventoryCompany        = each.ContractAndLineLocation.Company 
										invoked.InventoryLocation       = each.ContractAndLineLocation.Location 
										invoked.RequestingLocation      = each.ContractAndLineLocation.RequestingLocation 
										invoked.ContractOrItemLocation  = 2
								else 
								if (each.ContractAndLineLocation.RequestingLocation entered
								and RequestingLocForInventoryRel !exists)
									invoke Create ConsolidatedItemLocation 
										invoked.ContractGroup			= ContractGroup 
										invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
										invoked.InventoryCompany        = each.ContractAndLineLocation.Company 
										invoked.InventoryLocation       = each.ContractAndLineLocation.Location 
										invoked.RequestingLocation      = each.ContractAndLineLocation.RequestingLocation 
										invoked.ContractOrItemLocation  = 2						
				LocalFromCreate = true 
				if (InternalItem exists)
				 	include UpdateFromItem 
				else
				if (ContractLineRel exists)
					include UpdateFromContract 

				if (HasGTIN)
					include BuildTextIndexes		

				if (InternalItem !exists
				and	ContractLineRel exists)
					initialize ConsolidatedVendorItemOccurs
					for each OpenContractLinesRel 
						if (each.Contract.HasAPurchaseType)
							invoke MaintainVendorItems
								invoked.ParmVendor       = each.Vendor 
								invoked.ParmVendorItem   = each.VendorItem 

		UpdateAndConsolidate is an Update Action 
			restricted 
			Action Rules 

				if (!TransientFromSuggestedItem)
					invoke Update TransientNewConsolidatedItemMaster
						invoked.TransientContract       				= TransientContract 
						invoked.TransientContractLine   				= TransientContractLine
						invoked.TransientNewItem    					= TransientNewItem 	

			Exit Rules 

				if (!TransientFromSuggestedItem
				or   ContractLinesByManufacturerRel !exists)			
					NewConsolidatedItemMaster   = TransientNewConsolidatedItemMaster
					UpdateDate					= current corporate date
					initialize ConsolidatedVendorItemOccurs
					ManufacturerInfoChangedFlag = false
					InternalItemChangedFlag     = false 
					OnHoldFlag                  = false 		
					HasErrorsFlag               = false 		
					ClosedFlag                  = false         
					NotForClinicalUseFlag       = false		
					ClinicalSystemUse           = false         
					NotAvailableForUseFlag      = false         

		UpdateForItemNotInUseWithContract is an Instance Action 
			restricted 
			Action Rules 

				InternalItem 				= first OpenContractLinesRel.ItemNumber 
				InternalItemChangedFlag 	= true
				UpdateDate                  = current corporate date
		
		Update is an Update Action 
			restricted
			Action Rules 
				if (ConsolidatedItemMasterUOM !exists)
					invoke Create ConsolidatedItemMasterUOM
           				invoked.ContractGroup           = ContractGroup
            			invoked.ConsolidatedItemMaster  = ConsolidatedItemMaster
				if (CreateDate != current corporate date)
					UpdateDate		= current corporate date
				LocalContract		= TransientContract 
				LocalContractLine   = TransientContractLine 
				if (TransientCostChange)
					CostUpdatedFlag = true 
				if (TransientNewItem entered)
					InternalItem 			= TransientNewItem 
					InternalItemChangedFlag = true
				if  (TransientDeletedFromItem
				and  ContractLinesByManufacturerRel !exists)
					DeletedFlag = true
				else	
				if (InternalItem exists)
					include UpdateFromItem 
					for each ConsolidatedLocationRel 
						if (each.ContractOrItemLocation = 1)
							invoke Update each 
				else 
				if (ContractLineRel exists)
				 	include UpdateFromContract
				if  (TransientContract entered)
					for each ContractAndLineLocationRel 
						LocalCompany 				= each.ContractAndLineLocation.Company 
						LocalLocation          		= each.ContractAndLineLocation.Location 
						LocalRequestingLocation     = each.ContractAndLineLocation.RequestingLocation 
						if (ConsolidatedItemLocationExistsRel !exists
						and ClinicalSystemUse)
							invoke Create ConsolidatedItemLocation 
								invoked.ContractGroup			= ContractGroup 
								invoked.ConsolidatedItemMaster	= ConsolidatedItemMaster
								invoked.InventoryCompany        = each.ContractAndLineLocation.Company 
								invoked.InventoryLocation       = each.ContractAndLineLocation.Location 
								invoked.RequestingLocation      = each.ContractAndLineLocation.RequestingLocation 
								invoked.ContractOrItemLocation  = 2
						else 
						if (ConsolidatedItemLocationExistsRel exists)
							for each ConsolidatedItemLocationExistsRel
								if (each.ContractOrItemLocation = 2)
									invoke Update each 

			Exit Rules 

				if (HasGTIN)
					include BuildTextIndexes	

				if (InternalItem !exists
				and ConsolidatedItemLocationRel exists)
					if (first ConsolidatedItemLocationRel.DefaultUnitCostContractLine entered)
						LocalContract		= first ConsolidatedItemLocationRel.DefaultUnitCostContract
						LocalContractLine	= first ConsolidatedItemLocationRel.DefaultUnitCostContractLine 
						InternalItem 		= ContractLineRel.ItemNumber 
				
				if (InternalItem !exists
				and	ContractLineRel exists)
