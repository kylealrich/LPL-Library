				LocalFinanceDimension8 = FinanceDimension8
			else
				while (LocalFinanceDimension8.Level > LocalLevel)
					LocalFinanceDimension8			= LocalDimensionHierarchyRel.FinanceDimension8Hierarchy.ParentDimension

	Derived Fields
