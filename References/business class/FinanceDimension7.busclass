			valid when (SummaryOrPostingDimension)
			
			Parameters
				PrmContextDimension is a FinanceDimension7ContextValue
				NewParent 			is a FinanceDimension7
							
			Parameter Rules
				PrmContextDimension
					initial value is FinanceDimension7
					
				NewParent
					default to DerivedTopNode
					required
					constraint (NewParent != FinanceDimension7)
						"CannotMoveToSelf"
					constraint (NewParent.DimensionType.Summary
					or			NewParent.DimensionType.Node)
						"ParentMustBeASummaryNode"
					constraint (NewParent.DimensionStructure = FinanceDimension7Structure)
						"ParentDoesNotBelongToThisStructure"
					constraint (NewParent != DerivedParentDimension)
						"<FinanceDimension7.Description>IsAlreadyAParent<FinanceEnterpriseGroup.FinanceDimension7Label>"	
					TopNodeFound = false
					LocalFinanceDimension7 = NewParent
					if (!NewParent.DimensionType.Node)
						while (!TopNodeFound)
							constraint (FinanceDimension7 != LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension)
								"<NewParent>MayNotBeMovedToADescendantOfItsOwnBranch" 
							if (LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
							or  LocalFinanceDimension7 not entered)
								TopNodeFound = true
							else
								LocalFinanceDimension7 = LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension	
													
			Local Fields
				OldParent is like FinanceDimension7
																		
			Entrance Rules
				include CheckValidContext
							
				OldParent				 = DerivedParentDimension
				ParentDimension			 = NewParent

			Action Rules
				ParentDimension  	 = NewParent
				invoke Update DimensionInContextStructureRel
					invoked.ParentDimension		 	= NewParent

			Exit Rules
 				LocalFinanceDimension7 = FinanceDimension7
 							

				include CreateShadowForDimension		

				if (DimensionType.Summary)
					TopNodeFound = false
					while (!TopNodeFound)
						if (LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.DimensionType.Summary)
							if (BudgetIdentifier entered)	
								constraint (BudgetIdentifier != LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier)
									"BudgetIdentifier<BudgetIdentifier>AlreadyInUseBy<LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.DisplayDimension>"  
							if (LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier entered)
								I1 += 1
								BudgetIdentifierArray.BudgetIdentifier[I1] = LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.BudgetIdentifier				

						if (LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension.DimensionType.Node
						or  LocalFinanceDimension7 not entered)
							TopNodeFound = true
						else
							LocalFinanceDimension7 = LocalFinanceDimension7.DimensionInContextStructureRel.ParentDimension	
						
					invoke WalkDownHierarchy
						invoked.PrmBudgetIdentifierArray = BudgetIdentifierArray	
						invoked.PrmBudgetIdentifierCount = I1					
				
					invoke NotifyMoveDimension7 NotifyBudgetTemplateRel
						invoked.PrmFinanceDimension7 = FinanceDimension7
						invoked.PrmNewParent		 = NewParent
						invoked.PrmOldParent		 = OldParent
				include TriggerBODForCreateOrUpdate
		
