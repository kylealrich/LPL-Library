		RequestNewFinanceDimension7 is a Create RequestAction
			default label is "RequestNew"
			action comment required
			request action process is RequestNewFEGDimensions
			request action linkback webapp is FinancialBusinessAnalyst
			allow user fields as parameters
			Parameters
				RequesterActor				is an Actor
			 	RequestedBy					is Alpha 100
				RequesterPhone				is a Telephone
			 	RequesterEmail				is an EmailAddress
				RequestPriority				is Numeric 1
					States
						High				value is 1
						Medium				value is 2
						Low					value is 3
				NeededBy					is Date
				FinanceEnterpriseGroup
				CreateDimensionType			is Numeric 2
					States
			    		Summary	value is 2
			    		Posting	value is 1
			    FinanceDimension7Structure
					default label is "<FinanceEnterpriseGroup.FinanceDimension7Label>Structure"
				DisplayDimension 				is like FinanceDimension7
					translatable
				PrmFinanceDimension7			is like FinanceDimension7
				ParentFinanceDimension7			is a FinanceDimension7
				Description			
				BudgetIdentifier
				Active
				SystemDimension					is Boolean
				IncludeInBudgetEdit				is Boolean
				AddressCode
				
			Parameter Rules
				RequestPriority
					required
				NeededBy
					required
				RequesterActor
					initial value is actor
					default to actor
				RequestedBy
					initial value is RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
					default to RequesterActor.PersonName.GivenName + " " + RequesterActor.PersonName.FamilyName
				RequesterPhone
					initial value is RequesterActor.ContactInfo.TelephoneNumber
					default to RequesterActor.ContactInfo.TelephoneNumber
				RequesterEmail
					initial value is RequesterActor.ContactInfo.EmailAddress
					default to RequesterActor.ContactInfo.EmailAddress
				FinanceDimension7Structure
					initial value is FinanceEnterpriseGroup.EnterpriseDim7Structure
					default to FinanceEnterpriseGroup.EnterpriseDim7Structure
				DisplayDimension
					default to PrmFinanceDimension7
				PrmFinanceDimension7
					required
						"Field<FinanceEnterpriseGroup.FinanceDimension7Label>Required"
					if (FinanceEnterpriseGroup.FinanceDimension7RegularExpression entered
					and CreateDimensionType.Posting)
						constraint (PrmFinanceDimension7 matches FinanceEnterpriseGroup.FinanceDimension7RegularExpression)
							"InvalidFormatFor<FinanceEnterpriseGroup.FinanceDimension7Label>"
					LocalStructure 					= FinanceDimension7Structure
					LocalFinanceEnterpriseGroup 	= FinanceEnterpriseGroup
					LocalDisplayDimension			= PrmFinanceDimension7
					constraint (!RequestDisplayDimensionRel exists)
						"DisplayValueAlreadyExistsFor<first RequestDisplayDimensionRel.FinanceDimension7>"
				CreateDimensionType		
					required
				BudgetIdentifier
					constraint (CreateDimensionType.Summary)
						"BudgetIdentifierOnlyValidForSummaryDimensions"					
				IncludeInBudgetEdit
					constraint (CreateDimensionType.Posting)
						"NotValidForSummaryDimension"
				SystemDimension
					constraint (CreateDimensionType.Posting)
						"MustBeAPostingType<FinanceEnterpriseGroup.FinanceDimension7Label>"	
					constraint (FinanceDimension7Structure.DerivedSystemDimension = FinanceDimension7Structure.UndefinedMsg
						or FinanceDimension7Structure.DerivedSystemDimension = PrmFinanceDimension7)
						"<FinanceDimension7Structure.DerivedSystemDimension>IsAlreadyDesignatedAsSystem<FinanceEnterpriseGroup.FinanceDimension7Label>"
				Description
					required
				Active
					initial value is true
					if (SystemDimension)
						Active = true
			
			Action Rules		
				invoke Create this instance
					fill in user fields from parameters	
					invoked.FinanceEnterpriseGroup 		= FinanceEnterpriseGroup
					invoked.DimensionType				= CreateDimensionType
					invoked.FinanceDimension7			= PrmFinanceDimension7
					invoked.Description					= Description
					invoked.DisplayDimension 			= PrmFinanceDimension7
					invoked.BudgetIdentifier			= BudgetIdentifier
					invoked.SystemDimension				= SystemDimension
					invoked.Active						= Active
					invoked.IncludeInBudgetEdit			= IncludeInBudgetEdit
					invoked.AddressCode					= AddressCode
					if (ParentFinanceDimension7 entered)
						invoked.ParentDimension			= ParentFinanceDimension7
						invoked.SkipPropagate		  	= true
						invoked.InvokedByCreateWithin 	= true
