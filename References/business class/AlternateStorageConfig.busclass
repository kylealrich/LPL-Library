AlternateStorageConfig is a BusinessClass
	owned by la
	prefix is ASC

	Ontology
		symbolic key is AlternateStorageConfig

	Persistent Fields
		StorageTarget is Alpha size 1
			States
				Database = blank
				S3       = "S"
				    default label is "S\3"
		LastRun is TimeStamp

	Field Rules
		StorageTarget
			initial value is StorageTarget.Database
			if (not DeveloperMode)
				cannot be changed
					"InternalUseOnly"

	Derived Fields
		DeveloperMode is a NativeField
			type is Boolean
